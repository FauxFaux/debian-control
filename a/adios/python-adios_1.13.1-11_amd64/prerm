#!/bin/sh

set -e

if [ "$1" != "upgrade" ]; then
 (update-alternatives --remove-all python-adios-x86_64-linux-gnu ) || true
fi


# Automatically added by dh_python2:
if which pyclean >/dev/null 2>&1; then
	pyclean -p python-adios 
else
	dpkg -L python-adios | grep '\.py$' | while read file
	do
		rm -f "${file}"[co] >/dev/null
  	done
fi

# End automatically added section


exit 0
