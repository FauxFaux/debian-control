Depends: mlock, libc6 (>= 2.15), libgssapi-krb5-2 (>= 1.14+dfsg), libkrb5-3 (>= 1.6.dfsg.2), libldap-2.4-2 (>= 2.4.7), libpam0g (>= 0.99.7.1), libssl1.1 (>= 1.1.0), libtinfo6 (>= 6)
