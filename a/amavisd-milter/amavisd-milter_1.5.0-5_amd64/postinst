#!/bin/sh
set -e
# Automatically added by dh_installinit
if [ -x "/etc/init.d/amavisd-milter" ]; then
	update-rc.d amavisd-milter defaults >/dev/null
	if [ -n "$2" ]; then
		_dh_action=restart
	else
		_dh_action=start
	fi
	invoke-rc.d amavisd-milter $_dh_action || exit $?
fi
# End automatically added section
