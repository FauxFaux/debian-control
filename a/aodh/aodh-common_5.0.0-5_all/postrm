#!/bin/sh

set -e

if [ "${1}" = "purge" ] ; then
	rm -rf /etc/aodh /var/lib/aodh /var/log/aodh
fi

# Automatically added by dh_installdebconf/10.10.9
if [ "$1" = purge ] && [ -e /usr/share/debconf/confmodule ]; then
	. /usr/share/debconf/confmodule
	db_purge
fi
# End automatically added section


exit 0
