Depends: arb-common (= 6.0.6-4), fastdnaml, libarb (= 6.0.6-4), openssh-client, mafft, mrbayes, phylip, phyml, raxml, readseq, fig2dev, tree-puzzle, xdg-utils, libc6 (>= 2.15), libgcc1 (>= 1:3.0), libglib2.0-0 (>= 2.12.0), libstdc++6 (>= 5.2), libx11-6, libxext6, libxm4 (>= 2.3.3), libxpm4, libxt6
