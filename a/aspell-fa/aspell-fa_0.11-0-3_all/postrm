#!/bin/sh
set -e
# Automatically added by installdeb-aspell
dico_class="aspell"
dico_varlib_remove="fa.compat fa-common.rws fa-generic.rws fa-scientific.rws"

case "$1" in abort-install|remove)
	if [ -d "/var/lib/$dico_class" ]; then
	    ( cd "/var/lib/$dico_class" && rm -f $dico_varlib_remove )
	fi
esac
# End automatically added section
# Automatically added by installdeb-aspell
rmscript="update-dictcommon-aspell"

case "$1" in abort-install|remove)
	if which $rmscript > /dev/null 2>&1; then
	    $rmscript 
	else
	    echo "Warning: $rmscript not present or executable." >&2
	fi
esac
# End automatically added section
