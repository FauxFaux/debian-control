#!/bin/sh
set -e
# Automatically added by dh_systemd_start/11
if [ -d /run/systemd/system ]; then
	systemctl --system daemon-reload >/dev/null || true
fi
# End automatically added section
# Automatically added by dh_systemd_enable/11
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask 'autosuspend-detect-suspend.service' 'autosuspend.service' >/dev/null
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge 'autosuspend-detect-suspend.service' 'autosuspend.service' >/dev/null
		deb-systemd-helper unmask 'autosuspend-detect-suspend.service' 'autosuspend.service' >/dev/null
	fi
fi
# End automatically added section
