#!/bin/sh

set -e

DOCDIR=/usr/share/doc/libavifile-0.7c2

case "$1" in
    install|upgrade)
	if [ -d $DOCDIR ]; then
	    rm -rf $DOCDIR
	fi
    ;;

    abort-upgrade)
    ;;

    *)
        echo "preinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

# Automatically added by dh_installdeb/11.3.5
dpkg-maintscript-helper symlink_to_dir /usr/share/doc/libavifile-0.7c2 /usr/share/doc/libavifile-0.7-common 1:0.7.48~20090503.ds-16.1~ -- "$@"
# End automatically added section


exit 0
