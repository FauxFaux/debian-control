#! /bin/sh

set -e

THISDB=pgsql

if [ -f /usr/share/debconf/confmodule ]; then
	. /usr/share/debconf/confmodule
        db_version 2.0
fi

if [ -f /usr/share/dbconfig-common/dpkg/postrm.$THISDB ]; then
	. /usr/share/dbconfig-common/dpkg/postrm.$THISDB
        dbc_go bacula-director-$THISDB $@
fi

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

# Automatically added by dh_installdeb/11.5.4
dpkg-maintscript-helper symlink_to_dir /usr/share/doc/bacula-director-pgsql bacula-common 9.4.0-1\~ -- "$@"
# End automatically added section
# Automatically added by dh_installdebconf/11.5.4
if [ "$1" = purge ] && [ -e /usr/share/debconf/confmodule ]; then
	. /usr/share/debconf/confmodule
	db_purge
fi
# End automatically added section


exit 0
