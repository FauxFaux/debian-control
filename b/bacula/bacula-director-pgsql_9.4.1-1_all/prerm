#! /bin/sh

set -e


. /usr/share/debconf/confmodule
db_version 2.0


THISDB=pgsql
if [ -f /usr/share/dbconfig-common/dpkg/prerm.$THISDB ]; then
	. /usr/share/dbconfig-common/dpkg/prerm.$THISDB
	dbc_go bacula-director-$THISDB $@
fi

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

# Automatically added by dh_installdeb/11.5.4
dpkg-maintscript-helper symlink_to_dir /usr/share/doc/bacula-director-pgsql bacula-common 9.4.0-1\~ -- "$@"
# End automatically added section


exit 0
