Depends: dbconfig-common, postgresql-client, ucf, debconf (>= 0.5) | debconf-2.0, libc6 (>= 2.14), libgd3 (>= 2.1.0~alpha~), libpcap0.8 (>= 0.9.8), libpng16-16 (>= 1.6.2-1), libpq5
