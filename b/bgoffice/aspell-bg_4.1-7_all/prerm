#!/bin/sh

set -e

case "$1" in
    remove|deconfigure)
        DIR="/usr/lib/aspell"
        update-alternatives --remove bulgarian-aspell-dictionary $DIR/bg-w_english.multi
        update-alternatives --remove bulgarian-aspell-dictionary $DIR/bg-wo_english.multi
	;;
    
    upgrade|failed-upgrade)
	;;
    
    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
	;;
esac



exit 0
