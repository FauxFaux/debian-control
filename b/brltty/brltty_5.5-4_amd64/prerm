#!/bin/sh

set -e

for i in brl-sk-all.ktb
do
	dpkg-maintscript-helper rm_conffile /etc/brltty/$i 4.4-9~ brltty -- "$@"
done

# Automatically added by dh_installinit/10.10.9
if [ -x "/etc/init.d/brltty" ] && [ "$1" = remove ]; then
	invoke-rc.d brltty stop || exit $?
fi
# End automatically added section



