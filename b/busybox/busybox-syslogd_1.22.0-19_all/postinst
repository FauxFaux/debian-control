#!/bin/sh
set -e
# Automatically added by dh_installinit
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/busybox-syslogd" ]; then
		update-rc.d busybox-syslogd defaults >/dev/null
		invoke-rc.d busybox-syslogd start || exit $?
	fi
fi
# End automatically added section
# Automatically added by dh_installinit
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/busybox-klogd" ]; then
		update-rc.d busybox-klogd defaults >/dev/null
		invoke-rc.d busybox-klogd start || exit $?
	fi
fi
# End automatically added section
