#!/bin/sh
set -e
# Automatically added by dh_installinit/10.10.9
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/busybox-syslogd" ]; then
		update-rc.d busybox-syslogd defaults >/dev/null
		if [ -n "$2" ]; then
			_dh_action=restart
		else
			_dh_action=start
		fi
		invoke-rc.d busybox-syslogd $_dh_action || exit $?
	fi
fi
# End automatically added section
# Automatically added by dh_installinit/10.10.9
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/busybox-klogd" ]; then
		update-rc.d busybox-klogd defaults >/dev/null
		if [ -n "$2" ]; then
			_dh_action=restart
		else
			_dh_action=start
		fi
		invoke-rc.d busybox-klogd $_dh_action || exit $?
	fi
fi
# End automatically added section
