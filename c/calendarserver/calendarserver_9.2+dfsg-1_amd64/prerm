#!/bin/sh
set -e
# Automatically added by dh_installinit/11.5.4
if [ -x "/etc/init.d/calendarserver" ]; then
	invoke-rc.d calendarserver stop || exit 1
fi
# End automatically added section

# Automatically added by dh_python2:
if which pyclean >/dev/null 2>&1; then
	pyclean -p calendarserver 
else
	dpkg -L calendarserver | grep '\.py$' | while read file
	do
		rm -f "${file}"[co] >/dev/null
  	done
fi

# End automatically added section
