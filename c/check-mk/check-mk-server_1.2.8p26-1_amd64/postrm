#!/bin/sh

set -e

# cleanup var dirs on 
if [ "$1" = "purge" ] ; then
        [ -e /var/lib/check_mk ] && rm -rf /var/lib/check_mk || true
fi

# Automatically added by dh_installdebconf/10.9
if [ "$1" = purge ] && [ -e /usr/share/debconf/confmodule ]; then
	. /usr/share/debconf/confmodule
	db_purge
fi
# End automatically added section

