#!/bin/sh

set -e


case "$1" in
    remove|purge)
    ;;

    install|upgrade|failed-upgrade)
        if dpkg --compare-versions "$2" le "7.2-2"; then
            [ -e /var/lib/dpkg/info/chrome-gnome-shell.postrm ] && rm /var/lib/dpkg/info/chrome-gnome-shell.postrm
        fi
    ;;

    *)
        echo "Called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# Automatically added by dh_installdeb/11.3.5
dpkg-maintscript-helper rm_conffile /etc/opt/chrome/policies/managed/chrome-gnome-shell.json 10-2\~ -- "$@"
dpkg-maintscript-helper rm_conffile /etc/chromium/policies/managed/chrome-gnome-shell.json 10-2\~ -- "$@"
# End automatically added section


exit 0
