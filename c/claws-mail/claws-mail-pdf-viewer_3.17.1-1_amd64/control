Depends: libatk1.0-0 (>= 1.12.4), libc6 (>= 2.4), libcairo2 (>= 1.2.4), libcurl3-gnutls (>= 7.16.2), libdb5.3, libetpan20 (>= 1.0), libexpat1 (>= 2.0.1), libfontconfig1 (>= 2.12.6), libfreetype6 (>= 2.2.1), libgdk-pixbuf2.0-0 (>= 2.22.0), libglib2.0-0 (>= 2.16.0), libgnutls30 (>= 3.5.0), libgtk2.0-0 (>= 2.18.0), liblockfile1 (>= 1.0), libpango-1.0-0 (>= 1.14.0), libpangocairo-1.0-0 (>= 1.14.0), libpangoft2-1.0-0 (>= 1.14.0), libpoppler-glib8 (>= 0.18.0), libsasl2-2, zlib1g (>= 1:1.1.4), claws-mail (= 3.17.1-1)
