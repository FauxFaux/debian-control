#!/bin/sh
# postrm script for libmigemo1
#
# see: dh_installdeb(1)

set -e

case "$1" in
    remove)
        rm -f /usr/share/cmigemo/cp932/migemo-dict
        rm -f /usr/share/cmigemo/euc-jp/migemo-dict
        rm -f /usr/share/cmigemo/utf-8/migemo-dict
        ;;
    upgrade|deconfigure|failed-upgrade)
        ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
        ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.



exit 0
