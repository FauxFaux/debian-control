#! /bin/sh
# postinst script for dbab
#
# see: dh_installdeb(1)

set -e

# http://www.debian.org/doc/debian-policy/ch-opersys.html#s9.3.3.1
if [ "$1" = purge ]; then
  update-rc.d -f dbab remove > /dev/null
  rm -f /etc/dnsmasq.d/dbab.* > /dev/null || true
fi

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

# Automatically added by dh_installsystemd/11.4
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask 'dbab.service' >/dev/null || true
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge 'dbab.service' >/dev/null || true
		deb-systemd-helper unmask 'dbab.service' >/dev/null || true
	fi
fi
# End automatically added section


exit 0

