#!/bin/sh
# prerm script for dicod
#

set -e

case "$1" in
    remove)
      [ ! -x /usr/sbin/update-inetd ] || update-inetd --remove dict
    ;;
    upgrade|deconfigure)
      [ ! -x /usr/sbin/update-inetd ] || update-inetd --disable dict
    ;;
    failed-upgrade)
    ;;
    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 0
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

# Automatically added by dh_installinit
if [ -x "/etc/init.d/dicod" ]; then
	invoke-rc.d dicod stop || exit $?
fi
# End automatically added section


exit 0
