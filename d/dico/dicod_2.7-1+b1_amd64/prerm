#!/bin/sh
# prerm script for dicod
#

set -e

case "$1" in
    remove)
      ! which update-inetd > /dev/null || update-inetd --remove dict
    ;;
    upgrade|deconfigure)
      ! which update-inetd > /dev/null || update-inetd --disable dict
    ;;
    failed-upgrade)
    ;;
    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 0
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

# Automatically added by dh_installinit/11.5.4
if [ -x "/etc/init.d/dicod" ] && [ "$1" = remove ]; then
	invoke-rc.d dicod stop || exit 1
fi
# End automatically added section


exit 0
