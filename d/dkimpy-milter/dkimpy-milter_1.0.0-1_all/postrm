#!/bin/sh
set -e
# Automatically added by dh_installsystemd/11.2.1
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask 'dkimpy-milter.service' >/dev/null || true
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge 'dkimpy-milter.service' >/dev/null || true
		deb-systemd-helper unmask 'dkimpy-milter.service' >/dev/null || true
	fi
fi
# End automatically added section
