#!/bin/sh
set -e
# Automatically added by dh_installxmlcatalogs
if [ "$1" = "remove" ] || [ "$1" = "upgrade" ]; then
	update-xmlcatalog --del --type public --id "-//Norman Walsh//DTD Slides" --root || true
	update-xmlcatalog --del --type system --id "http://docbook.sourceforge.net/release/slides/" --root || true

	update-xmlcatalog --del --type public --id "-//Norman Walsh//DTD Slides" --package docbook-slides || true
	update-xmlcatalog --del --type system --id "http://docbook.sourceforge.net/release/slides/" --package docbook-slides || true

fi
# End automatically added section
