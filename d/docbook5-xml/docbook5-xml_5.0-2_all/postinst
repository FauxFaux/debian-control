#!/bin/sh
set -e
# Automatically added by dh_installxmlcatalogs
if [ "$1" = "configure" ]; then
	update-xmlcatalog --add --type system --id "http://www.oasis-open.org/docbook/xml/5.0/" --package docbook5-xml --local /usr/share/xml/docbook/schema/catalog-docbook5.xml
	update-xmlcatalog --add --type system --id "http://docbook.org/xml/5.0/" --package docbook5-xml --local /usr/share/xml/docbook/schema/catalog-docbook5.xml
	update-xmlcatalog --add --type public --id "-//OASIS//DTD DocBook XML 5//EN" --package docbook5-xml --local /usr/share/xml/docbook/schema/catalog-docbook5.xml
	update-xmlcatalog --add --type public --id "-//OASIS//DTD DocBook XML 5.0//EN" --package docbook5-xml --local /usr/share/xml/docbook/schema/catalog-docbook5.xml

	update-xmlcatalog --add --type system --id "http://www.oasis-open.org/docbook/xml/5" --package docbook5-xml --root
	update-xmlcatalog --add --type system --id "http://docbook.org/xml/5" --package docbook5-xml --root
	update-xmlcatalog --add --type public --id "-//OASIS//DTD DocBook XML 5" --package docbook5-xml --root

fi
# End automatically added section
