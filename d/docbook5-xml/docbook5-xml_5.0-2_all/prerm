#!/bin/sh
set -e
# Automatically added by dh_installxmlcatalogs
if [ "$1" = "remove" ] || [ "$1" = "upgrade" ]; then
	update-xmlcatalog --del --type system --id "http://www.oasis-open.org/docbook/xml/5" --root || true
	update-xmlcatalog --del --type system --id "http://docbook.org/xml/5" --root || true
	update-xmlcatalog --del --type public --id "-//OASIS//DTD DocBook XML 5" --root || true

	update-xmlcatalog --del --type system --id "http://www.oasis-open.org/docbook/xml/5.0/" --package docbook5-xml || true
	update-xmlcatalog --del --type system --id "http://docbook.org/xml/5.0/" --package docbook5-xml || true
	update-xmlcatalog --del --type public --id "-//OASIS//DTD DocBook XML 5//EN" --package docbook5-xml || true
	update-xmlcatalog --del --type public --id "-//OASIS//DTD DocBook XML 5.0//EN" --package docbook5-xml || true

fi
# End automatically added section
