#!/bin/bash -e

case "$1" in
  upgrade|install)
    if update-alternatives --query epix &>/dev/null
    then
	update-alternatives --remove-all epix || true
    fi
    ;;
esac

# Automatically added by dh_installemacsen/10.8
if ( [ "$1" = "install" ] || [ "$1" = "upgrade" ] ) \
	&& [ -e /var/lib/emacsen-common/state/package/installed/emacsen-common -a -x /usr/lib/emacsen-common/emacs-package-install ]
then
	/usr/lib/emacsen-common/emacs-package-install --preinst epix
fi
# End automatically added section
# Automatically added by dh_installdeb/10.8
dpkg-maintscript-helper rm_conffile /etc/bash_completion.d/epix 1.2.16-2\~ -- "$@"
# End automatically added section


exit 0
