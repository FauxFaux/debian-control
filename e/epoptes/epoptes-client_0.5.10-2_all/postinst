#!/bin/sh
set -e
# Automatically added by dh_installinit
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/epoptes-client" ]; then
		update-rc.d epoptes-client defaults 30 >/dev/null
		invoke-rc.d epoptes-client start || exit $?
	fi
fi
# End automatically added section
