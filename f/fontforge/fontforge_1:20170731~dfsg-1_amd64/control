Depends: fontforge-common (= 1:20170731~dfsg-1), libfontforge2 (= 1:20170731~dfsg-1), libgdraw5 (= 1:20170731~dfsg-1), libc6 (>= 2.2.5), libltdl7 (>= 2.4.6), libx11-6
