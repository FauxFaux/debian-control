Depends: java-wrappers (>= 0.1.15), libavalon-framework-java (>= 4.1.2-2), libbatik-java (>= 1.8), libbsf-java, libcommons-io-java, libcommons-logging-java, libfontbox-java, libxalan2-java, libxerces2-java, libxml-commons-external-java, libxmlgraphics-commons-java (>= 2.1), libxt6, libxtst6
