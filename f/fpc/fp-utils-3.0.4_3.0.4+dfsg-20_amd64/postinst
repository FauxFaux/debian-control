	--slave /usr/bin/ppufiles ppufiles /usr/bin/ppufiles-3.0.4 \
	--slave /usr/bin/ppudump ppudump /usr/bin/ppudump-3.0.4 \
	--slave /usr/bin/ppumove ppumove /usr/bin/ppumove-3.0.4 \
