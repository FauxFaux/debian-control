#! /bin/sh

set -e

FPCDIR=/usr/lib/x86_64-linux-gnu/fpc

# add alternatives
update-alternatives \
    --install ${FPCDIR}/default fp-utils ${FPCDIR}/3.0.4 3000400 \
    --slave ${FPCDIR}/lexyacc fplexyacc ${FPCDIR}/3.0.4/lexyacc \
	--slave /usr/bin/chmcmd chmcmd /usr/bin/chmcmd-3.0.4 \
