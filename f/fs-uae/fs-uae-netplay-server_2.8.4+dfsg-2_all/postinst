# Automatically added by dh_installinit/11.5.3
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ] ; then
	if [ -x "/etc/init.d/fs-uae-netplay-server" ]; then
		update-rc.d fs-uae-netplay-server defaults >/dev/null
		invoke-rc.d fs-uae-netplay-server start || exit 1
