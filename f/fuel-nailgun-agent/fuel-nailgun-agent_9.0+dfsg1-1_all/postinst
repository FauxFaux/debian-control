#!/bin/sh
set -e
# Automatically added by dh_installinit
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/fuel-nailgun-agent" ]; then
		update-rc.d fuel-nailgun-agent defaults >/dev/null
		invoke-rc.d fuel-nailgun-agent start || true
	fi
fi
# End automatically added section
