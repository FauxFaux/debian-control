#! /bin/sh -e

if [ "$1" = "remove" ] || [ "$1" = "deconfigure" ]; then
    update-alternatives --quiet --remove java /usr/bin/gij-5
    update-alternatives --quiet --remove rmiregistry /usr/bin/grmiregistry-5
    update-alternatives --quiet --remove keytool /usr/bin/gkeytool-5
    update-alternatives --quiet --remove orbd /usr/bin/gorbd-5
    update-alternatives --quiet --remove rmid /usr/bin/grmid-5
fi



exit 0
