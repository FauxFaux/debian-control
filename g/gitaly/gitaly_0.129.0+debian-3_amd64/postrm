#!/bin/sh

set -e

# Automatically added by dh_installinit/11.5.3
if [ "$1" = "purge" ] ; then
	update-rc.d gitaly remove >/dev/null
fi
# End automatically added section
# Automatically added by dh_installsystemd/11.5.3
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask 'gitaly.service' >/dev/null || true
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge 'gitaly.service' >/dev/null || true
		deb-systemd-helper unmask 'gitaly.service' >/dev/null || true
	fi
fi
# End automatically added section


if [ "$1" = "purge" ]; then
  # Remove systemd service overrides
  rm -rf /etc/systemd/system/gitaly.service.d
fi
