#!/bin/sh

set -e

if ! /usr/bin/getent passwd gkrellmd >/dev/null; then
  useradd --system --no-user-group gkrellmd
fi

# Automatically added by dh_installinit
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/gkrellmd" ]; then
		update-rc.d gkrellmd defaults 21 >/dev/null
		invoke-rc.d gkrellmd start || exit $?
	fi
fi
# End automatically added section


exit 0
