#!/bin/sh
set -e
# Automatically added by dh_installinit/11.5.3
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ] ; then
	if [ -x "/etc/init.d/globus-scheduler-event-generator" ]; then
		update-rc.d globus-scheduler-event-generator defaults-disabled >/dev/null
		invoke-rc.d globus-scheduler-event-generator start || exit 1
	fi
fi
