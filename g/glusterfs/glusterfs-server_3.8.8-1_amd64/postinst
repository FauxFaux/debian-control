#!/bin/sh
set -e
# Automatically added by dh_installinit
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/glusterfs-server" ]; then
		update-rc.d glusterfs-server defaults >/dev/null
		invoke-rc.d glusterfs-server start || exit $?
	fi
fi
# End automatically added section
