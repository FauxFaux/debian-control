#!/bin/sh
set -e
# Automatically added by dh_systemd_enable/11.3.5
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ] ; then
	if deb-systemd-helper debian-installed 'glusterd.service'; then
		# This will only remove masks created by d-s-h on package removal.
		deb-systemd-helper unmask 'glusterd.service' >/dev/null || true

		if deb-systemd-helper --quiet was-enabled 'glusterd.service'; then
			# Create new symlinks, if any.
			deb-systemd-helper enable 'glusterd.service' >/dev/null || true
		fi
	fi

	# Update the statefile to add new symlinks (if any), which need to be cleaned
	# up on purge. Also remove old symlinks.
	deb-systemd-helper update-state 'glusterd.service' >/dev/null || true
fi
# End automatically added section
# Automatically added by dh_systemd_enable/11.3.5
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ] ; then
	if deb-systemd-helper debian-installed 'glusterd.service'; then
		# This will only remove masks created by d-s-h on package removal.
		deb-systemd-helper unmask 'glusterd.service' >/dev/null || true

		if deb-systemd-helper --quiet was-enabled 'glusterd.service'; then
			# Create new symlinks, if any.
			deb-systemd-helper enable 'glusterd.service' >/dev/null || true
		fi
	fi

	# Update the statefile to add new symlinks (if any), which need to be cleaned
	# up on purge. Also remove old symlinks.
	deb-systemd-helper update-state 'glusterd.service' >/dev/null || true
fi
# End automatically added section
# Automatically added by dh_systemd_enable/11.3.5
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ] ; then
	if deb-systemd-helper debian-installed 'glustereventsd.service'; then
		# This will only remove masks created by d-s-h on package removal.
		deb-systemd-helper unmask 'glustereventsd.service' >/dev/null || true

		if deb-systemd-helper --quiet was-enabled 'glustereventsd.service'; then
			# Create new symlinks, if any.
			deb-systemd-helper enable 'glustereventsd.service' >/dev/null || true
		fi
	fi

	# Update the statefile to add new symlinks (if any), which need to be cleaned
	# up on purge. Also remove old symlinks.
	deb-systemd-helper update-state 'glustereventsd.service' >/dev/null || true
fi
# End automatically added section
# Automatically added by dh_installinit/11.3.5
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ] ; then
	# In case this system is running systemd, we need to ensure that all
	# necessary tmpfiles (if any) are created before starting.
	if [ -d /run/systemd/system ] ; then
		systemd-tmpfiles --create glusterfs-server.conf >/dev/null || true
	fi
fi
# End automatically added section
# Automatically added by dh_systemd_start/11.3.5
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ] ; then
	if [ -d /run/systemd/system ]; then
		systemctl --system daemon-reload >/dev/null || true
		deb-systemd-invoke start 'glusterd.service' 'glustereventsd.service' >/dev/null || true
	fi
fi
# End automatically added section
# Automatically added by dh_installdeb/11.3.5
dpkg-maintscript-helper rm_conffile /etc/init.d/glusterfs-server 3.10.1-2 -- "$@"
# End automatically added section
# Automatically added by dh_installdeb/11.3.5
dpkg-maintscript-helper rm_conffile /etc/init.d/glusterd 4.0.1-1 -- "$@"
# End automatically added section
# Automatically added by dh_installdeb/11.3.5
dpkg-maintscript-helper rm_conffile /etc/init.d/glustereventsd 4.0.1-1 -- "$@"
# End automatically added section
