#!/bin/sh
set -e


if [ "$1" = "remove" ] || [ "$1" = "deconfigure" ]; then

	update-alternatives --remove glx /usr/lib/nvidia/bumblebee
	update-alternatives --remove glx /usr/lib/nvidia

fi


# Automatically added by dh_installdeb/11.3.5
