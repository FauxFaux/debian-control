Depends: libc6 (>= 2.4), libcairo2 (>= 1.2.4), libgee-0.8-2 (>= 0.8.3), libglib2.0-0 (>= 2.40.0), libgnome-games-support-1-3 (>= 1.1.90), libgtk-3-0 (>= 3.19.0), librsvg2-2 (>= 2.32.0), dconf-gsettings-backend | gsettings-backend
