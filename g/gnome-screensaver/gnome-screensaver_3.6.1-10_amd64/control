Depends: libc6 (>= 2.7), libcairo2 (>= 1.10.0), libdbus-1-3 (>= 1.9.14), libdbus-glib-1-2 (>= 0.78), libgdk-pixbuf2.0-0 (>= 2.22.0), libglib2.0-0 (>= 2.37.3), libgnome-desktop-3-17 (>= 3.17.92), libgnomekbd8 (>= 3.6.0), libgtk-3-0 (>= 3.0.0), libpam0g (>= 0.99.7.1), libsystemd0, libx11-6, libxext6, libxklavier16 (>= 5.0), libxxf86vm1, default-dbus-session-bus | dbus-session-bus, gsettings-desktop-schemas (>= 2.91.92)
