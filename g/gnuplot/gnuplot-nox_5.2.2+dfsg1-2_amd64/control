Depends: gnuplot-data (= 5.2.2+dfsg1-2), libc6 (>= 2.23), libcairo2 (>= 1.6.0), libedit2 (>= 2.11-20080614), libgd3 (>= 2.1.0~alpha~), libglib2.0-0 (>= 2.12.0), liblua5.3-0, libpango-1.0-0 (>= 1.22.0), libpangocairo-1.0-0 (>= 1.14.0)
