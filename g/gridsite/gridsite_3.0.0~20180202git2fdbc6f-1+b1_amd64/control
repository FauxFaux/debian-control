Depends: libgridsite6 (= 3.0.0~20180202git2fdbc6f-1+b1), apache2-bin (>= 2.4.16), libc6 (>= 2.15), libcanl-c4, libgsoap-2.8.60, libssl1.1 (>= 1.1.0), apache2-api-20120211, apache2-api-20120211-openssl1.1
