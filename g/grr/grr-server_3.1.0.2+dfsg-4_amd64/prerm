#!/bin/sh
set -e
# Automatically added by dh_systemd_start/10.7.2
if [ -d /run/systemd/system ]; then
	deb-systemd-invoke stop grr-server.service >/dev/null
fi
# End automatically added section
# Automatically added by dh_installinit/10.7.2
if [ -x "/etc/init.d/grr-server@" ]; then
	invoke-rc.d grr-server@ stop || exit $?
fi
# End automatically added section
# Automatically added by dh_installinit/10.7.2
if [ -x "/etc/init.d/grr-server" ]; then
	invoke-rc.d grr-server stop || exit $?
fi
# End automatically added section

# Automatically added by dh_python2:
if which pyclean >/dev/null 2>&1; then
	pyclean -p grr-server 
else
	dpkg -L grr-server | grep \.py$ | while read file
	do
		rm -f "${file}"[co] >/dev/null
  	done
fi

# End automatically added section
