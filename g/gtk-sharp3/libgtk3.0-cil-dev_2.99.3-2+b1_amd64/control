Depends: libatk3.0-cil (= 2.99.3-2+b1), libcairo1.10-cil (= 2.99.3-2+b1), libgdk3.0-cil-dev (= 2.99.3-2+b1), libgio3.0-cil-dev (= 2.99.3-2+b1), libglib3.0-cil-dev (= 2.99.3-2+b1), libgtk3.0-cil (= 2.99.3-2+b1), libpango3.0-cil (= 2.99.3-2+b1)
