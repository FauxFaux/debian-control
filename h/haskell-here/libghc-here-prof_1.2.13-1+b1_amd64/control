Depends: libghc-here-dev (= 1.2.13-1+b1), libghc-base-prof-4.10.1.0-35a7f, libghc-haskell-src-meta-prof-0.8.0.3-5f523, libghc-mtl-prof-2.2.2-ada79, libghc-parsec-prof-3.1.13.0-c7e67, libghc-template-haskell-prof-2.12.0.0-22576
