#!/bin/sh
set -e
# Automatically added by dh_installinit/11.5.3
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ] ; then
	if [ -x "/etc/init.d/heimdal-kcm" ]; then
		update-rc.d heimdal-kcm defaults >/dev/null
		if [ -n "$2" ]; then
			_dh_action=restart
		else
			_dh_action=start
		fi
		invoke-rc.d heimdal-kcm $_dh_action || exit 1
	fi
fi
# End automatically added section
