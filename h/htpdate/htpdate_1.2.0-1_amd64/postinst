#!/bin/sh
set -e
# Automatically added by dh_installinit/10.9
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/htpdate" ]; then
		update-rc.d htpdate defaults >/dev/null
		if [ -n "$2" ]; then
			_dh_action=restart
		else
			_dh_action=start
		fi
		invoke-rc.d htpdate $_dh_action || exit $?
	fi
fi
# End automatically added section
