#!/bin/sh

set -e

if [ "$1" = "purge" ]; then
	rm -f /etc/default/i2p
	rm -rf /var/lib/i2p
	rm -rf /var/log/i2p
	rm -rf /etc/i2p
fi

# Automatically added by dh_installinit/11.5.3
if [ "$1" = "purge" ] ; then
	update-rc.d i2p remove >/dev/null
fi
# End automatically added section
# Automatically added by dh_installdebconf/11.5.3
if [ "$1" = purge ] && [ -e /usr/share/debconf/confmodule ]; then
	. /usr/share/debconf/confmodule
	db_purge
fi
# End automatically added section
# Automatically added by dh_apparmor/2.13.1-3
if [ "$1" = "purge" ] && ! [ -e "/etc/apparmor.d/usr.bin.i2prouter" ] ; then
    rm -f "/etc/apparmor.d/disable/usr.bin.i2prouter" || true
    rm -f "/etc/apparmor.d/force-complain/usr.bin.i2prouter" || true
    rm -f "/etc/apparmor.d/local/usr.bin.i2prouter" || true
    rmdir /etc/apparmor.d/disable 2>/dev/null || true
    rmdir /etc/apparmor.d/local   2>/dev/null || true
    rmdir /etc/apparmor.d         2>/dev/null || true
fi
# End automatically added section
# Automatically added by dh_apparmor/2.13.1-3
if [ "$1" = "purge" ] && ! [ -e "/etc/apparmor.d/system_i2p" ] ; then
    rm -f "/etc/apparmor.d/disable/system_i2p" || true
    rm -f "/etc/apparmor.d/force-complain/system_i2p" || true
    rm -f "/etc/apparmor.d/local/system_i2p" || true
    rmdir /etc/apparmor.d/disable 2>/dev/null || true
    rmdir /etc/apparmor.d/local   2>/dev/null || true
    rmdir /etc/apparmor.d         2>/dev/null || true
fi
# End automatically added section


exit 0
