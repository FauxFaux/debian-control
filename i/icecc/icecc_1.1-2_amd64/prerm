#!/bin/sh
# prerm script for icecc

set -e

# Automatically added by dh_installdeb/10.9
dpkg-maintscript-helper rm_conffile /etc/init.d/icecc 1.0.0-1~ icecc -- "$@"
# End automatically added section
# Automatically added by dh_installinit/10.9
if [ -x "/etc/init.d/iceccd" ]; then
	invoke-rc.d iceccd stop || exit $?
fi
# End automatically added section


if [ -x "/etc/init.d/icecc-scheduler" ]; then
        invoke-rc.d icecc-scheduler stop || exit $?
fi

exit 0
