#!/bin/sh

set -e


if [ "$1" = "remove" ]
then
    # remove all alternatives
    for prog in compare animate convert composite conjure import identify stream display montage mogrify; do
	update-alternatives --remove $prog /usr/bin/$prog-im6.q16hdri
	update-alternatives --remove $prog-im6 /usr/bin/$prog-im6.q16hdri
    done
fi



# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.


