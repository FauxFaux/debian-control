#!/bin/sh
set -e
# Automatically added by dh_apparmor/2.11.1-3
if [ "$1" = "purge" ] && ! [ -e "/etc/apparmor.d/usr.lib.rtcw" ] ; then
    rm -f "/etc/apparmor.d/disable/usr.lib.rtcw" || true
    rm -f "/etc/apparmor.d/force-complain/usr.lib.rtcw" || true
    rm -f "/etc/apparmor.d/local/usr.lib.rtcw" || true
    rmdir /etc/apparmor.d/disable 2>/dev/null || true
    rmdir /etc/apparmor.d/local   2>/dev/null || true
    rmdir /etc/apparmor.d         2>/dev/null || true
fi
# End automatically added section
