#!/bin/sh
#    Debian postrm script
#
set -e

case "$1" in
	purge)
		if [ -d /var/lib/ipmiutil/ ]
		then
			rm -Rf /var/lib/ipmiutil/*
		fi
	;;
	remove|upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
	;;
	*)
		echo "postrm called with unknown argument \`$1'" >&2
		exit 1
	;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

# Automatically added by dh_installdeb
dpkg-maintscript-helper rm_conffile /etc/init.d/ipmi_port 3.0.0-3\~ -- "$@"
dpkg-maintscript-helper rm_conffile /etc/init.d/ipmiutil_asy 3.0.0-3\~ -- "$@"
dpkg-maintscript-helper rm_conffile /etc/init.d/ipmiutil_evt 3.0.0-3\~ -- "$@"
dpkg-maintscript-helper rm_conffile /etc/init.d/ipmiutil_wdt 3.0.0-3\~ -- "$@"
# End automatically added section
# Automatically added by dh_systemd_start
if [ -d /run/systemd/system ]; then
	systemctl --system daemon-reload >/dev/null || true
fi
# End automatically added section
# Automatically added by dh_systemd_start
if [ -d /run/systemd/system ]; then
	systemctl --system daemon-reload >/dev/null || true
fi
# End automatically added section
# Automatically added by dh_systemd_start
if [ -d /run/systemd/system ]; then
	systemctl --system daemon-reload >/dev/null || true
fi
# End automatically added section
# Automatically added by dh_systemd_enable
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask ipmi_port.service ipmiutil_asy.service ipmiutil_evt.service ipmiutil_wdt.service >/dev/null
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge ipmi_port.service ipmiutil_asy.service ipmiutil_evt.service ipmiutil_wdt.service >/dev/null
		deb-systemd-helper unmask ipmi_port.service ipmiutil_asy.service ipmiutil_evt.service ipmiutil_wdt.service >/dev/null
	fi
fi
# End automatically added section
# Automatically added by dh_systemd_enable
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask ipmi_port.service ipmiutil_asy.service ipmiutil_evt.service >/dev/null
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge ipmi_port.service ipmiutil_asy.service ipmiutil_evt.service >/dev/null
		deb-systemd-helper unmask ipmi_port.service ipmiutil_asy.service ipmiutil_evt.service >/dev/null
	fi
fi
# End automatically added section
# Automatically added by dh_systemd_enable
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask ipmi_port.service ipmiutil_asy.service >/dev/null
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge ipmi_port.service ipmiutil_asy.service >/dev/null
		deb-systemd-helper unmask ipmi_port.service ipmiutil_asy.service >/dev/null
	fi
fi
# End automatically added section
# Automatically added by dh_systemd_enable
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask ipmi_port.service >/dev/null
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge ipmi_port.service >/dev/null
		deb-systemd-helper unmask ipmi_port.service >/dev/null
	fi
fi
# End automatically added section
# Automatically added by dh_systemd_enable
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask ipmi_port.service >/dev/null
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge ipmi_port.service >/dev/null
		deb-systemd-helper unmask ipmi_port.service >/dev/null
	fi
fi
# End automatically added section


exit 0
