#!/bin/sh
set -e
# Automatically added by dh_installinit/11.3.5
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ] ; then
	if [ -x "/etc/init.d/kea-dhcp-ddns-server" ]; then
		update-rc.d kea-dhcp-ddns-server defaults >/dev/null
		invoke-rc.d kea-dhcp-ddns-server start || exit 1
	fi
fi
