#! /bin/sh
# prerm script for isoqlog
#
# see: dh_installdeb(1)

set -e


case "$1" in
    remove|upgrade|deconfigure)
	rm -f /etc/cron.daily/isoqlog
	rm -f /etc/isoqlog/isoqlog.conf
	rm -f /etc/isoqlog/isoqlog.domains
	
        ;;
    failed-upgrade)
        ;;
    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.



exit 0


