#!/bin/sh

set -e

if [ "$1" = "remove" ] ; then
	update-alternatives --remove jenkins-jobs /usr/bin/python2-jenkins-jobs
fi


# Automatically added by dh_python2:
if which pyclean >/dev/null 2>&1; then
	pyclean -p python-jenkins-job-builder 
else
	dpkg -L python-jenkins-job-builder | grep '\.py$' | while read file
	do
		rm -f "${file}"[co] >/dev/null
  	done
fi

# End automatically added section


exit 0
