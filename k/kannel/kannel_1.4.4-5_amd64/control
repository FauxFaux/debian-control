Depends: libc6 (>= 2.14), libct4 (>= 0.64), libhiredis0.13 (>= 0.13.1), libmariadbclient18 (>= 5.5.36), libpam0g (>= 0.99.7.1), libpcre3, libpq5, libsqlite0 (>= 2.8.17), libsqlite3-0 (>= 3.5.9), libssl1.0.2 (>= 1.0.2d), libxml2 (>= 2.7.4), zlib1g (>= 1:1.1.4), adduser, psmisc
