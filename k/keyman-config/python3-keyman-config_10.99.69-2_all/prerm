#!/bin/sh
set -e
# Automatically added by dh_usrlocal/12
(
	while read dir; do
		rmdir "$dir" 2>/dev/null || true
	done
) << DATA
/usr/local/share/doc/keyman
/usr/local/share/doc
/usr/local/share/fonts/keyman
/usr/local/share/fonts
/usr/local/share/keyman
DATA
# End automatically added section

# Automatically added by dh_python3:
if which py3clean >/dev/null 2>&1; then
	py3clean -p python3-keyman-config 
else
	dpkg -L python3-keyman-config | perl -ne 's,/([^/]*)\.py$,/__pycache__/\1.*, or next; unlink $_ or die $! foreach glob($_)'
	find /usr/lib/python3/dist-packages/ -type d -name __pycache__ -empty -print0 | xargs --null --no-run-if-empty rmdir
fi

# End automatically added section
