#!/bin/sh
set -e

update-alternatives --install /usr/bin/kinput2 kinput2 /usr/bin/kinput2-canna 30 --slave /usr/share/man/man1/kinput2.1x.gz kinput2.1x.gz /usr/share/man/man1/kinput2-canna.1x.gz

update-alternatives \
        --install /etc/X11/xinit/xinput.d/ja_JP xinput-ja_JP \
        /etc/X11/xinit/xinput.d/kinput2-canna 70

# Automatically added by dh_installmenu
if [ "$1" = "configure" ] && [ -x "`which update-menus 2>/dev/null`" ]; then
	update-menus
fi
# End automatically added section


