#!/bin/sh
set -e

# Automatically added by dh_python3:
if which py3compile >/dev/null 2>&1; then
	py3compile -p kopano-presence 
fi
if which pypy3compile >/dev/null 2>&1; then
	pypy3compile -p kopano-presence  || true
fi

# End automatically added section
# Automatically added by dh_installinit/11.5.4
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ] ; then
	if [ -x "/etc/init.d/kopano-presence" ]; then
		update-rc.d kopano-presence defaults >/dev/null
		if [ -n "$2" ]; then
			_dh_action=restart
		else
			_dh_action=start
		fi
		invoke-rc.d --skip-systemd-native kopano-presence $_dh_action || exit 1
	fi
fi
# End automatically added section
