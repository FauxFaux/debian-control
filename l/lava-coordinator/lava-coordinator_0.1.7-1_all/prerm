#!/bin/sh
set -e
# Automatically added by dh_installinit
if [ -x "/etc/init.d/lava-coordinator" ]; then
	invoke-rc.d lava-coordinator stop || exit $?
fi
# End automatically added section

# Automatically added by dhpython:
if which pyclean >/dev/null 2>&1; then
	pyclean -p lava-coordinator 
else
	dpkg -L lava-coordinator | grep \.py$ | while read file
	do
		rm -f "${file}"[co] >/dev/null
  	done
fi

# End automatically added section
