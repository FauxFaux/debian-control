#!/bin/sh
# prerm script for levee
set -e

case "$1" in
    remove|upgrade|deconfigure)
	update-alternatives --remove editor /usr/bin/levee
	update-alternatives --remove vi /usr/bin/levee
	;;

    failed-upgrade)
	;;

    *)
        echo "prerm called with unknown argument: $1" >&2
        exit 1
	;;
esac



exit 0
