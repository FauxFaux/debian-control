#!/bin/sh
set -e
# Automatically added by dh_installinit/11.1.2
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/linux-igd" ]; then
		update-rc.d linux-igd defaults >/dev/null
		if [ -n "$2" ]; then
			_dh_action=restart
		else
			_dh_action=start
		fi
		invoke-rc.d linux-igd $_dh_action || exit 1
	fi
fi
# End automatically added section
