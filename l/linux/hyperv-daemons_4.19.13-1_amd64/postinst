# Automatically added by dh_installinit/12
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ] ; then
	if [ -x "/etc/init.d/hyperv-daemons.hv-fcopy-daemon" ]; then
		update-rc.d hyperv-daemons.hv-fcopy-daemon defaults >/dev/null
		invoke-rc.d hyperv-daemons.hv-fcopy-daemon start || exit 1
	fi
fi
