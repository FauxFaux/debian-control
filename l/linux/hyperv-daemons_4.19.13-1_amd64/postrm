#!/bin/sh
set -e
# Automatically added by dh_installinit/12
if [ "$1" = "purge" ] ; then
	update-rc.d hyperv-daemons.hv-vss-daemon remove >/dev/null
fi
# End automatically added section
# Automatically added by dh_installinit/12
if [ "$1" = "purge" ] ; then
	update-rc.d hyperv-daemons.hv-kvp-daemon remove >/dev/null
fi
# End automatically added section
# Automatically added by dh_installinit/12
if [ "$1" = "purge" ] ; then
	update-rc.d hyperv-daemons.hv-fcopy-daemon remove >/dev/null
fi
# End automatically added section
# Automatically added by dh_systemd_enable/12
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask 'hyperv-daemons.hv-fcopy-daemon.service' 'hyperv-daemons.hv-kvp-daemon.service' 'hyperv-daemons.hv-vss-daemon.service' >/dev/null || true
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge 'hyperv-daemons.hv-fcopy-daemon.service' 'hyperv-daemons.hv-kvp-daemon.service' 'hyperv-daemons.hv-vss-daemon.service' >/dev/null || true
		deb-systemd-helper unmask 'hyperv-daemons.hv-fcopy-daemon.service' 'hyperv-daemons.hv-kvp-daemon.service' 'hyperv-daemons.hv-vss-daemon.service' >/dev/null || true
	fi
fi
# End automatically added section
