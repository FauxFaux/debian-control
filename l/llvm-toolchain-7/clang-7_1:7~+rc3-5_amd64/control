Depends: libc6 (>= 2.14), libgcc1 (>= 1:3.4), libllvm7 (>= 1:7~svn298832-1~), libstdc++6 (>= 5.2), libstdc++-8-dev, libgcc-8-dev, libobjc-8-dev, libclang-common-7-dev (= 1:7~+rc3-5), libclang1-7 (= 1:7~+rc3-5), libc6-dev, binutils
