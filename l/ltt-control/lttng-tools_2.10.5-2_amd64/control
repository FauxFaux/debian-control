Depends: libc6 (>= 2.15), libkmod2 (>= 5~), liblttng-ctl0 (>= 2.10.0), liblttng-ust-ctl4 (>= 2.10.0), libpopt0 (>= 1.14), liburcu6 (>= 0.10.0), libuuid1 (>= 2.16), libxml2 (>= 2.7.4), lsb-base (>= 3.0-6)
