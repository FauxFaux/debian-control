#!/bin/sh
set -e
# Automatically added by dh_installinit/11.5.3
if [ "$1" = "purge" ] ; then
	update-rc.d lxcfs remove >/dev/null
fi
# End automatically added section
# Automatically added by dh_installdeb/11.5.3
dpkg-maintscript-helper rm_conffile /etc/init/lxcfs.conf 3.0.3-2\~ lxcfs -- "$@"
# End automatically added section
# Automatically added by dh_installsystemd/11.5.3
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask 'lxcfs.service' >/dev/null || true
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge 'lxcfs.service' >/dev/null || true
		deb-systemd-helper unmask 'lxcfs.service' >/dev/null || true
	fi
fi
# End automatically added section
