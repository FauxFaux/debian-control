Depends: libchamplain-0.12-0 (= 0.12.16-3), gir1.2-champlain-0.12 (= 0.12.16-3), libglib2.0-dev, libclutter-1.0-dev, libsqlite3-dev, libcairo2-dev (>= 1.4), libsoup2.4-dev (>= 2.42)
