Depends: perl, libconfig-model-perl (>= 2.114), libfile-homedir-perl, liblog-log4perl-perl, libpath-tiny-perl, libpod-pom-perl, libtext-diff-perl, libtk-dirselect-perl, libtk-doubleclick-perl, libtk-fontdialog-perl, libtk-pod-perl, libtry-tiny-perl, libyaml-perl, perl-tk
