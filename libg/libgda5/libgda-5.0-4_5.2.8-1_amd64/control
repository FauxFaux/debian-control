Depends: libgda-5.0-common (>= 5.2.8-1), libc6 (>= 2.14), libglib2.0-0 (>= 2.41.1), libsoup2.4-1 (>= 2.4.1), libsqlite3-0 (>= 3.5.9), libxml2 (>= 2.7.4), libxslt1.1 (>= 1.1.25)
