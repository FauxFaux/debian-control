#!/bin/sh

set -e

case "$1" in
    configure|reconfigure)
        rm -f /usr/lib/guestfs/supermin.d/base.img.gz
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# Automatically added by dh_installdeb/11.3.5
