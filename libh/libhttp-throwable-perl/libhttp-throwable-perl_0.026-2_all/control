Depends: liblist-allutils-perl, libmoox-strictconstructor-perl, libpackage-variant-perl, libplack-perl, libthrowable-perl, libsub-exporter-perl, libtype-tiny-perl, libnamespace-clean-perl, libmoo-perl (>= 1.004003), perl
