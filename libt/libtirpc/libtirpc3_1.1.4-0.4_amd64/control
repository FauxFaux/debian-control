Depends: libc6 (>= 2.16), libcom-err2 (>= 1.43.9), libgssapi-krb5-2 (>= 1.14+dfsg), libk5crypto3 (>= 1.6.dfsg.2), libkrb5-3 (>= 1.6.dfsg.2), libtirpc-common (>= 1.1.4-0.4)
