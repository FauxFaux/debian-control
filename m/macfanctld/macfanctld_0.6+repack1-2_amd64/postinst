#!/bin/sh
set -e
# Automatically added by dh_installinit/11.3.1
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ] ; then
	if [ -x "/etc/init.d/macfanctld" ]; then
		update-rc.d macfanctld defaults >/dev/null
		invoke-rc.d macfanctld start || exit 1
	fi
fi
# End automatically added section
