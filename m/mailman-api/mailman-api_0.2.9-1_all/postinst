#!/bin/sh
set -e

# Automatically added by dhpython:
if which pycompile >/dev/null 2>&1; then
	pycompile -p mailman-api 
fi

# End automatically added section
# Automatically added by dh_installinit
if [ -x "/etc/init.d/mailman-api" ]; then
	update-rc.d mailman-api defaults >/dev/null
	invoke-rc.d mailman-api start || exit $?
fi
# End automatically added section
