#!/bin/sh
set -e
# Automatically added by dh_usrlocal/10.7.2
if [ "$1" = configure ]; then
(
	while read line; do
		set -- $line
		dir="$1"; mode="$2"; user="$3"; group="$4"
		if [ ! -e "$dir" ]; then
			if mkdir "$dir" 2>/dev/null; then
				chown "$user":"$group" "$dir"
				chmod "$mode" "$dir"
			fi
		fi
	done
) << DATA
/usr/local/share 2775 root staff
/usr/local/share/games 2775 root staff
/usr/local/share/games/mame 2775 root staff
/usr/local/share/games/mame/artwork 2775 root staff
/usr/local/share/games/mame/cheat 2775 root staff
/usr/local/share/games/mame/crosshair 2775 root staff
/usr/local/share/games/mame/hash 2775 root staff
/usr/local/share/games/mame/roms 2775 root staff
/usr/local/share/games/mame/samples 2775 root staff
DATA
fi
# End automatically added section
