#!/bin/sh
set -e
# Automatically added by dh_installinit
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/maradns-zoneserver" ]; then
		update-rc.d maradns-zoneserver defaults >/dev/null
		invoke-rc.d maradns-zoneserver start || exit $?
	fi
fi
# End automatically added section
