Depends: gir1.2-mate-panel, gsettings-desktop-schemas, gvfs, mate-applets-common (= 1.20.2-2), mate-panel, python, python-gi, libatk1.0-0 (>= 1.12.4), libc6 (>= 2.15), libcairo-gobject2 (>= 1.10.0), libcairo2 (>= 1.2.4), libcpufreq0 (>= 001), libdbus-1-3 (>= 1.9.14), libdbus-glib-1-2 (>= 0.88), libgdk-pixbuf2.0-0 (>= 2.22.0), libglib2.0-0 (>= 2.37.3), libgtk-3-0 (>= 3.21.4), libgtksourceview-3.0-1 (>= 2.91.4), libgtop-2.0-11 (>= 2.22.3), libgucharmap-2-90-7 (>= 1:3.0.0), libice6 (>= 1:1.0.0), libiw30 (>= 30~pre1), libmate-panel-applet-4-1 (>= 1.6.1), libmateweather1 (>= 1.6.2), libnotify4 (>= 0.7.0), libpango-1.0-0 (>= 1.14.0), libpangocairo-1.0-0 (>= 1.14.0), libpolkit-gobject-1-0 (>= 0.99), libsm6, libupower-glib3 (>= 0.99.0), libwnck-3-0 (>= 2.91.6), libx11-6, libxml2 (>= 2.7.4)
