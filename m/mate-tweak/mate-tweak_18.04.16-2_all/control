Depends: dconf-cli, gir1.2-gtk-3.0, gir1.2-notify-0.7, mate-panel (>= 1.18.0), mesa-utils, python3 (>= 3.3), python3-gi, python3-psutil, python3-setproctitle, x11-xserver-utils, python3-pkg-resources, python3:any (>= 3.3~)
