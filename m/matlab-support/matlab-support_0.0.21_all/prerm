#!/bin/sh
set -e


case "$1" in
    remove)
        # check whether there are any configured alternatives
        # and if, remove them all
        if [ -n "$(update-alternatives --list matlab 2> /dev/null || true)" ]; then
            update-alternatives --remove-all matlab
        fi
    ;;
    remove|upgrade|deconfigure)
    ;;

    failed-upgrade)
    ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.



exit 0
