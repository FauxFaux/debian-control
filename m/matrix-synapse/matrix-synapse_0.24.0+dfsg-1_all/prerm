#!/bin/sh
set -e
# Automatically added by dh_installinit/10.10.3
if [ -x "/etc/init.d/matrix-synapse" ]; then
	invoke-rc.d matrix-synapse stop || exit $?
fi
# End automatically added section

# Automatically added by dh_python2:
if which pyclean >/dev/null 2>&1; then
	pyclean -p matrix-synapse 
else
	dpkg -L matrix-synapse | grep \.py$ | while read file
	do
		rm -f "${file}"[co] >/dev/null
  	done
fi

# End automatically added section
