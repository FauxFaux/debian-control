Depends: default-jre (>= 2:1.8) | java8-runtime, java-wrappers, libjgoodies-forms-java (>= 1.6), libcommons-compress-java, libcommons-lang3-java, libjackson2-core-java, libjchart2d-java, libjide-oss-java, libmac-widgets-java, libokhttp-java, libopenjfx-java, libswingx-java, libtimingframework-java, libxz-java
Recommends: flvstreamer, vlc | mpv | mplayer
