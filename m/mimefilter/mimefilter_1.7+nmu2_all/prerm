set -o posix

case "${1}" in
    (failed-upgrade|deconfigure)
        		:
    ;;
    (remove|upgrade)
        	:
    ;;
    (*)
	echo >&2 "${0##*/}: Called with unknown argument \`${1}'."
    ;;
esac

exit 0
