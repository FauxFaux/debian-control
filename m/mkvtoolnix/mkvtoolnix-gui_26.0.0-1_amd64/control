Depends: mkvtoolnix (= 26.0.0-1), libboost-filesystem1.62.0, libboost-regex1.62.0 (>= 1.62.0+dfsg-5.1), libboost-system1.62.0, libc6 (>= 2.14), libcmark0, libebml4v5 (>= 1.3.4-2~), libgcc1 (>= 1:3.0), libmagic1 (>= 5.12), libmatroska6v5 (>= 1.4.5), libpugixml1v5 (>= 1.7), libqt5core5a (>= 5.11.0~rc1), libqt5gui5 (>= 5.8.0), libqt5multimedia5 (>= 5.6.0~beta), libqt5network5 (>= 5.0.2), libqt5widgets5 (>= 5.11.0~rc1), libstdc++6 (>= 5.2), zlib1g (>= 1:1.1.4)
