#! /bin/sh
set -e
 
# font alternatives
ALT_NAME="fonts-japanese-gothic"
FONT_ENTRY="/usr/share/fonts/truetype/mona/mona.ttf"

remove_alternate_font_entry()
{
    update-alternatives --remove $ALT_NAME.ttf $FONT_ENTRY
}

case "$1" in
    upgrade|deconfigure)
    	;;
    remove)
        # alternative
	remove_alternate_font_entry
        ;;
    failed-upgrade)
        ;;
    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
        ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.



exit 0
