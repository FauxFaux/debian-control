#!/bin/sh
set -e

# Remove configuration files created by update-menus
if [ "$1" = "purge" ] ; then
    rm -rf /etc/X11/mwm
fi
# Automatically added by dh_installmenu/10.10.9
inst=/etc/menu-methods/mwm
if [ "$1" = "remove" ] && [ -f "$inst" ]; then chmod a-x $inst ; fi
if [ -x "`which update-menus 2>/dev/null`" ]; then update-menus ; fi
# End automatically added section

