#!/bin/sh
set -e
# Automatically added by dh_installinit
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/mysqmail-courier-logger" ]; then
		update-rc.d mysqmail-courier-logger defaults >/dev/null
		invoke-rc.d mysqmail-courier-logger start || exit $?
	fi
fi
# End automatically added section
