#!/bin/sh
set -e
# Automatically added by dh_systemd_enable/10.6.4
if deb-systemd-helper debian-installed snmptrapd.service; then
	# This will only remove masks created by d-s-h on package removal.
	deb-systemd-helper unmask snmptrapd.service >/dev/null || true

	if deb-systemd-helper --quiet was-enabled snmptrapd.service; then
		# Create new symlinks, if any.
		deb-systemd-helper enable snmptrapd.service >/dev/null || true
	fi
fi

# Update the statefile to add new symlinks (if any), which need to be cleaned
# up on purge. Also remove old symlinks.
deb-systemd-helper update-state snmptrapd.service >/dev/null || true
# End automatically added section
# Automatically added by dh_installinit/10.6.4
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/snmptrapd" ]; then
		update-rc.d snmptrapd defaults >/dev/null
		invoke-rc.d snmptrapd start || exit $?
	fi
fi
# End automatically added section
