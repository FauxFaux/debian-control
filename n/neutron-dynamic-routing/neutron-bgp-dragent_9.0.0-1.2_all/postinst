#!/bin/sh
set -e
# Automatically added by dh_installinit
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/neutron-bgp-dragent" ]; then
		update-rc.d neutron-bgp-dragent defaults >/dev/null
		invoke-rc.d neutron-bgp-dragent start || true
	fi
fi
# End automatically added section
