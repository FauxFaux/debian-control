#!/bin/sh
set -e
# Automatically added by dh_systemd_enable
if deb-systemd-helper debian-installed nomad.service; then
	# This will only remove masks created by d-s-h on package removal.
	deb-systemd-helper unmask nomad.service >/dev/null || true

	if deb-systemd-helper --quiet was-enabled nomad.service; then
		# Create new symlinks, if any.
		deb-systemd-helper enable nomad.service >/dev/null || true
	fi
fi

# Update the statefile to add new symlinks (if any), which need to be cleaned
# up on purge. Also remove old symlinks.
deb-systemd-helper update-state nomad.service >/dev/null || true
# End automatically added section
# Automatically added by dh_installinit
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/nomad" ]; then
		update-rc.d nomad defaults >/dev/null || exit $?
	fi
fi
# End automatically added section
