#!/bin/sh
set -e
# Automatically added by dh_installinit
if [ -x "/etc/init.d/acix-index" ]; then
	invoke-rc.d acix-index stop || exit $?
fi
# End automatically added section

# Automatically added by dh_python2:
if which pyclean >/dev/null 2>&1; then
	pyclean -p nordugrid-arc-acix-index 
else
	dpkg -L nordugrid-arc-acix-index | grep \.py$ | while read file
	do
		rm -f "${file}"[co] >/dev/null
  	done
fi

# End automatically added section
