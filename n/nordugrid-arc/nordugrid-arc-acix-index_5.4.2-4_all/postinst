#!/bin/sh

set -e


# Automatically added by dh_python2:
if which pycompile >/dev/null 2>&1; then
	pycompile -p nordugrid-arc-acix-index 
fi

# End automatically added section
# Automatically added by dh_installinit/11.3.5
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ] ; then
	if [ -x "/etc/init.d/acix-index" ]; then
		update-rc.d acix-index defaults-disabled >/dev/null
		invoke-rc.d acix-index start || exit 1
	fi
fi
# End automatically added section


dpkg-maintscript-helper rm_conffile /etc/default/acix-index 5.4.2-2 -- "$@"
