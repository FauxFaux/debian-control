#!/bin/sh
set -e


if [ "$1" = "remove" ] || [ "$1" = "deconfigure" ]; then

	update-alternatives --remove nvidia /usr/lib/nvidia/legacy-304xx
	dpkg-trigger --no-await register-glx-alternative-nvidia

fi


# Automatically added by dh_installdeb/11.1.3
dpkg-maintscript-helper rm_conffile /etc/nvidia/nvidia-legacy-304xx-modprobe.conf 304.131-2\~ -- "$@"
# End automatically added section

