#!/bin/sh
set -e

# Automatically added by dh_python2:
if which pycompile >/dev/null 2>&1; then
	pycompile -p python-nwsserver 
fi

# End automatically added section
# Automatically added by dh_installinit
if [ -x "/etc/init.d/nwsserver" ]; then
	update-rc.d nwsserver defaults >/dev/null
	invoke-rc.d nwsserver start || exit $?
fi
# End automatically added section
