#!/bin/sh
set -e

# Automatically added by dh_python2:
if which pyclean >/dev/null 2>&1; then
	pyclean -p python-nwsserver 
else
	dpkg -L python-nwsserver | grep \.py$ | while read file
	do
		rm -f "${file}"[co] >/dev/null
  	done
fi

# End automatically added section
# Automatically added by dh_installinit
if [ -x "/etc/init.d/nwsserver" ]; then
	invoke-rc.d nwsserver stop || exit $?
fi
# End automatically added section
