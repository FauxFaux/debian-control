Depends: libcamomile-ocaml-dev, libfileutils-ocaml-dev, libgettext-ocaml (= 0.3.7-1+b2), libcamlp4-ocaml-dev-w0ou6, libcamomile-ocaml-dev-no175, libfileutils-ocaml-dev-51qo0, libgettext-ocaml-z0e55, ocaml-nox-4.05.0
