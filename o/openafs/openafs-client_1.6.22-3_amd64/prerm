#!/bin/sh

set -e

if [ "$1" = remove ] || [ "$1" = deconfigure ] ; then
    update-alternatives --remove pagsh /usr/bin/pagsh.openafs
    update-alternatives --remove klog /usr/bin/klog.afs
fi

# Automatically added by dh_installinit/10.10.9
if [ -x "/etc/init.d/openafs-client" ] && [ "$1" = remove ]; then
	invoke-rc.d openafs-client stop || exit $?
fi
# End automatically added section


exit 0
