#!/bin/sh

set -e

if [ "$1" = remove ] || [ "$1" = deconfigure ] ; then
    update-alternatives --remove pagsh /usr/bin/pagsh.openafs
fi

# Automatically added by dh_installinit/11.3.5
if [ -x "/etc/init.d/openafs-client" ] && [ "$1" = remove ]; then
	invoke-rc.d openafs-client stop || exit 1
fi
# End automatically added section


exit 0
