#!/bin/sh

set -e

if [ "${1}" = "purge" ] ; then
	rm -rf /var/lib/openstack-cluster-installer-poc/runtime
	rm -rf /var/lib/openstack-cluster-installer-poc/templates
fi

# Automatically added by dh_installinit/11.5.3
if [ "$1" = "purge" ] ; then
	update-rc.d oci-poc-vms remove >/dev/null
fi
# End automatically added section
# Automatically added by dh_installinit/11.5.3
if [ "$1" = "purge" ] ; then
	update-rc.d oci-poc-virtual-network remove >/dev/null
fi
# End automatically added section


exit 0
