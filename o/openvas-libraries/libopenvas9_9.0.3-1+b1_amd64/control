Depends: libc6 (>= 2.15), libgcrypt20 (>= 1.8.0), libglib2.0-0 (>= 2.31.8), libgnutls30 (>= 3.5.6), libgpg-error0 (>= 1.14), libgpgme11 (>= 1.2.0), libhiredis0.14 (>= 0.14.0), libksba8 (>= 1.2.0), libldap-2.4-2 (>= 2.4.7), libpcap0.8 (>= 0.9.8), libradcli4, libsnmp30 (>= 5.7.3+dfsg-3~dfsg), libssh-4 (>= 0.8.0), libuuid1 (>= 2.16), zlib1g (>= 1:1.1.4)
