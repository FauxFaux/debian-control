#!/bin/sh
# prerm script for otags
#
# see: dh_installdeb(1)

set -e

case "$1" in
  remove|upgrade)
    rm -f /var/lib/otags/vi/TAGS
    rm -f /var/lib/otags/emacs/TAGS
    rm -f /var/lib/otags/TAGS
    rm -f /var/lib/otags/tags
  ;;

esac


# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.



exit 0
