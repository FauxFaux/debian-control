Depends: libghc-pandoc-sidenote-dev (= 0.19.0.0-2+b1), libghc-base-prof-4.11.1.0-5dd74, libghc-monad-gen-prof-0.3.0.1-f1c88, libghc-pandoc-prof-2.2.1-7865e, libghc-pandoc-types-prof-1.17.5.1-991d6
