#!/bin/sh
set -e

# Startup errors should never cause dpkg to fail.
initscript_error() {
    return 0
}

# Automatically added by dh_installinit
if [ -x "/etc/init.d/pdns-recursor" ] && [ "$1" = remove ]; then
	invoke-rc.d pdns-recursor stop || initscript_error
fi
# End automatically added section


exit 0
