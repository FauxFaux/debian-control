Depends: libedit2 (>= 2.11-20080614-4), libmagic1, libpcre3 (>= 2:8.35), mime-support, php7.2-common (= 7.2.9-1), php7.2-json, php7.2-opcache, php7.2-readline, tzdata, ucf, libargon2-1 (>= 0~20171227), libc6 (>= 2.27), libsodium23 (>= 1.0.14), libssl1.1 (>= 1.1.0), libxml2 (>= 2.8.0), zlib1g (>= 1:1.1.4)
