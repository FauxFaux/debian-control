Depends: libmagic1, mime-support, php7.3-cli, php7.3-common (= 7.3.0-2), php7.3-json, php7.3-opcache, tzdata, ucf, libargon2-1 (>= 0~20171227), libc6 (>= 2.27), libedit2 (>= 2.11-20080614-0), libpcre2-8-0 (>= 10.32), libsodium23 (>= 1.0.14), libssl1.1 (>= 1.1.0), libxml2 (>= 2.8.0), zlib1g (>= 1:1.1.4)
