Depends: libc6 (>= 2.14), libdevil1c2, libftgl2 (>= 2.1.3~rc5), libgcc1 (>= 1:3.0), libgl1, libglfw3 (>= 3.0), libglu1-mesa | libglu1, libilmbase23 (>= 2.2.1), libprotobuf10, libsdl-mixer1.2, libsdl1.2debian (>= 1.2.11), libsigc++-2.0-0v5 (>= 2.8.0), libstdc++6 (>= 5.2), libtinyxml2.6.2v5, pink-pony-data (= 1.4.1-2.1)
