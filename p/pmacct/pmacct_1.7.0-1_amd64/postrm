#!/bin/sh

set -e
if dpkg-maintscript-helper supports mv_conffile; then
    dpkg-maintscript-helper mv_conffile \
        /etc/init.d/pmacct \
        /etc/init.d/pmacctd \
        1.5.0-1~~ pmacct -- "$@"

    dpkg-maintscript-helper mv_conffile \
        /etc/default/pmacct \
        /etc/default/pmacctd \
        1.5.0-1~~ pmacct -- "$@"
fi

# Automatically added by dh_installinit/10.10
if [ "$1" = "purge" ] ; then
	update-rc.d uacctd remove >/dev/null
fi


# In case this system is running systemd, we make systemd reload the unit files
# to pick up changes.
if [ -d /run/systemd/system ] ; then
	systemctl --system daemon-reload >/dev/null || true
fi
# End automatically added section
# Automatically added by dh_installinit/10.10
if [ "$1" = "purge" ] ; then
	update-rc.d sfacctd remove >/dev/null
fi


# In case this system is running systemd, we make systemd reload the unit files
# to pick up changes.
if [ -d /run/systemd/system ] ; then
	systemctl --system daemon-reload >/dev/null || true
fi
# End automatically added section
# Automatically added by dh_installinit/10.10
if [ "$1" = "purge" ] ; then
	update-rc.d nfacctd remove >/dev/null
fi


# In case this system is running systemd, we make systemd reload the unit files
# to pick up changes.
if [ -d /run/systemd/system ] ; then
	systemctl --system daemon-reload >/dev/null || true
fi
# End automatically added section
# Automatically added by dh_installinit/10.10
if [ "$1" = "purge" ] ; then
	update-rc.d pmacctd remove >/dev/null
fi


# In case this system is running systemd, we make systemd reload the unit files
# to pick up changes.
if [ -d /run/systemd/system ] ; then
	systemctl --system daemon-reload >/dev/null || true
fi
# End automatically added section
# Automatically added by dh_systemd_enable/10.10
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask 'nfacctd.service' 'pmacctd.service' 'sfacctd.service' 'uacctd.service' >/dev/null
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge 'nfacctd.service' 'pmacctd.service' 'sfacctd.service' 'uacctd.service' >/dev/null
		deb-systemd-helper unmask 'nfacctd.service' 'pmacctd.service' 'sfacctd.service' 'uacctd.service' >/dev/null
	fi
fi
# End automatically added section
# Automatically added by dh_systemd_enable/10.10
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask 'nfacctd.service' 'pmacctd.service' 'sfacctd.service' >/dev/null
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge 'nfacctd.service' 'pmacctd.service' 'sfacctd.service' >/dev/null
		deb-systemd-helper unmask 'nfacctd.service' 'pmacctd.service' 'sfacctd.service' >/dev/null
	fi
fi
# End automatically added section
# Automatically added by dh_systemd_enable/10.10
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask 'nfacctd.service' 'pmacctd.service' >/dev/null
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge 'nfacctd.service' 'pmacctd.service' >/dev/null
		deb-systemd-helper unmask 'nfacctd.service' 'pmacctd.service' >/dev/null
	fi
fi
# End automatically added section
# Automatically added by dh_systemd_enable/10.10
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask 'pmacctd.service' >/dev/null
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge 'pmacctd.service' >/dev/null
		deb-systemd-helper unmask 'pmacctd.service' >/dev/null
	fi
fi
# End automatically added section


