#!/bin/sh
set -e
# Automatically added by dh_installinit/11.3.5
if [ -x "/etc/init.d/pokerth-server" ] && [ "$1" = remove ]; then
	invoke-rc.d pokerth-server stop || exit 1
fi
# End automatically added section
# Automatically added by dh_systemd_start/11.3.5
if [ -d /run/systemd/system ] && [ "$1" = remove ]; then
	deb-systemd-invoke stop 'pokerth-server.service' >/dev/null || true
fi
# End automatically added section
