# Automatically added by dh_installinit/11.5
if [ -x "/etc/init.d/prometheus-sql-exporter" ] && [ "$1" = remove ]; then
	invoke-rc.d prometheus-sql-exporter stop || exit 1
fi
# End automatically added section
# Automatically added by dh_systemd_start/11.5
if [ -d /run/systemd/system ] && [ "$1" = remove ]; then
	deb-systemd-invoke stop 'prometheus-sql-exporter.service' >/dev/null || true
fi
# End automatically added section
