#!/bin/sh

set -e

if [ "$1" = "configure" ] ; then
	update-alternatives --install /usr/bin/cinder cinder /usr/bin/python3-cinder 400
fi


# Automatically added by dh_python3:
if which py3compile >/dev/null 2>&1; then
	py3compile -p python3-cinderclient 
fi

# End automatically added section


exit 0
