#!/bin/sh

set -e

if [ "$1" = "configure" ] ; then
	update-alternatives --install /usr/bin/cloudkitty cloudkitty /usr/bin/python2-cloudkitty 300
fi


# Automatically added by dh_python2:
if which pycompile >/dev/null 2>&1; then
	pycompile -p python-cloudkittyclient 
fi

# End automatically added section


exit 0
