#!/bin/sh

set -e

if [ "$1" = "configure" ] ; then
	update-alternatives --install /usr/bin/cloudkitty cloudkitty /usr/bin/python3-cloudkitty 400
fi


# Automatically added by dh_python3:
if which py3compile >/dev/null 2>&1; then
	py3compile -p python3-cloudkittyclient 
fi

# End automatically added section


exit 0
