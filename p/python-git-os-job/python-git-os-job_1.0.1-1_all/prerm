#!/bin/sh

set -e

if [ "$1" = "remove" ] ; then
	update-alternatives --remove git-os-job /usr/bin/python2-git-os-job
fi


# Automatically added by dh_python2:
if which pyclean >/dev/null 2>&1; then
	pyclean -p python-git-os-job 
else
	dpkg -L python-git-os-job | grep \.py$ | while read file
	do
		rm -f "${file}"[co] >/dev/null
  	done
fi

# End automatically added section


exit 0
