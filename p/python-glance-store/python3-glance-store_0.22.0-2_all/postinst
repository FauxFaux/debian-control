#!/bin/sh

set -e

if [ "$1" = "configure" ] ; then
	update-alternatives --install /usr/bin/glance-rootwrap glance-rootwrap /usr/bin/python3-glance-rootwrap 200
fi


# Automatically added by dh_python3:
if which py3compile >/dev/null 2>&1; then
	py3compile -p python3-glance-store 
fi

# End automatically added section


exit 0
