#!/bin/sh

set -e

if [ "$1" = "configure" ] ; then
	update-alternatives --install /usr/bin/gnocchi gnocchi /usr/bin/python3-gnocchi 400
fi


# Automatically added by dh_python3:
if which py3compile >/dev/null 2>&1; then
	py3compile -p python3-gnocchiclient 
fi

# End automatically added section


exit 0
