#!/bin/sh

set -e

if [ "$1" = "remove" ] ; then
	update-alternatives --remove invoke /usr/bin/python2-invoke
	update-alternatives --remove inv /usr/bin/python2-inv
fi


# Automatically added by dhpython:
if which pyclean >/dev/null 2>&1; then
	pyclean -p python-invoke 
else
	dpkg -L python-invoke | grep \.py$ | while read file
	do
		rm -f "${file}"[co] >/dev/null
  	done
fi

# End automatically added section


exit 0

