#!/bin/sh

set -e

if [ "$1" = "configure" ] ; then
	update-alternatives --install /usr/bin/murano-pkg-check murano-pkg-check /usr/bin/python3-murano-pkg-check 200
fi


# Automatically added by dh_python3:
if which py3compile >/dev/null 2>&1; then
	py3compile -p python3-murano-pkg-check 
fi

# End automatically added section


exit 0
