#!/bin/sh

set -e

if [ "$1" = "configure" ] ; then
	update-alternatives --install /usr/bin/lockutils-wrapper lockutils-wrapper /usr/bin/python3-lockutils-wrapper 200
fi


# Automatically added by dh_python3:
if which py3compile >/dev/null 2>&1; then
	py3compile -p python3-oslo.concurrency 
fi

# End automatically added section


exit 0

