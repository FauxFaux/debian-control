#!/bin/sh

set -e

if [ "$1" = "remove" ] ; then
	update-alternatives --remove convert-json /usr/bin/python2-convert-json
fi


# Automatically added by dh_python2:
if which pyclean >/dev/null 2>&1; then
	pyclean -p python-oslo.log 
else
	dpkg -L python-oslo.log | grep \.py$ | while read file
	do
		rm -f "${file}"[co] >/dev/null
  	done
fi

# End automatically added section


exit 0
