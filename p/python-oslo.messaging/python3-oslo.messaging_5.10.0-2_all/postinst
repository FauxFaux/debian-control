#!/bin/sh

set -e

if [ "$1" = "configure" ] ; then
	update-alternatives --install /usr/bin/oslo-messaging-zmq-broker oslo-messaging-zmq-broker /usr/bin/python3-oslo-messaging-zmq-broker 200
	update-alternatives --install /usr/bin/oslo-messaging-zmq-proxy oslo-messaging-zmq-proxy /usr/bin/python3-oslo-messaging-zmq-proxy 200
fi


# Automatically added by dh_python3:
if which py3compile >/dev/null 2>&1; then
	py3compile -p python3-oslo.messaging 
fi

# End automatically added section


exit 0
