#!/bin/sh

set -e

if [ "$1" = "configure" ] ; then
	update-alternatives --install /usr/bin/gen_protorpc gen_protorpc /usr/bin/python2-gen_protorpc 300
fi


# Automatically added by dhpython:
if which pycompile >/dev/null 2>&1; then
	pycompile -p python-protorpc-standalone 
fi

# End automatically added section


exit 0
