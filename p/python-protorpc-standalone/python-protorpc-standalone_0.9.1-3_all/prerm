#!/bin/sh

set -e

if [ "$1" = "remove" ] ; then
	update-alternatives --remove gen_protorpc /usr/bin/python2-gen_protorpc
fi


# Automatically added by dhpython:
if which pyclean >/dev/null 2>&1; then
	pyclean -p python-protorpc-standalone 
else
	dpkg -L python-protorpc-standalone | grep \.py$ | while read file
	do
		rm -f "${file}"[co] >/dev/null
  	done
fi

# End automatically added section


exit 0
