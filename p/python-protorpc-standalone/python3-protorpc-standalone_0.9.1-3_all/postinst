#!/bin/sh

set -e

if [ "$1" = "configure" ] ; then
	update-alternatives --install /usr/bin/gen_protorpc gen_protorpc /usr/bin/python3-gen_protorpc 200
fi


# Automatically added by dhpython:
if which py3compile >/dev/null 2>&1; then
	py3compile -p python3-protorpc-standalone 
fi

# End automatically added section


exit 0
