#!/bin/sh

set -e

if [ "$1" = "remove" ] ; then
	update-alternatives --remove pulpdoctest /usr/bin/python2-pulpdoctest
	update-alternatives --remove pulptest /usr/bin/python2-pulptest
fi


# Automatically added by dhpython:
if which pyclean >/dev/null 2>&1; then
	pyclean -p python-pulp 
else
	dpkg -L python-pulp | grep \.py$ | while read file
	do
		rm -f "${file}"[co] >/dev/null
  	done
fi

# End automatically added section


exit 0
