#!/bin/sh
set -e
# Automatically added by dh_installinit
if [ -x "/etc/init.d/yhsm-validation-server" ]; then
	invoke-rc.d yhsm-validation-server stop || exit $?
fi
# End automatically added section

# Automatically added by dh_python2:
if which pyclean >/dev/null 2>&1; then
	pyclean -p yhsm-validation-server 
else
	dpkg -L yhsm-validation-server | grep \.py$ | while read file
	do
		rm -f "${file}"[co] >/dev/null
  	done
fi

# End automatically added section
