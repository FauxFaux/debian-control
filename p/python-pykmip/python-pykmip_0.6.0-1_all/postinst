#!/bin/sh

set -e

if [ "$1" = "configure" ] ; then
	update-alternatives --install /usr/bin/pykmip-server pykmip-server /usr/bin/python2-pykmip-server 300
fi


# Automatically added by dh_python2:
if which pycompile >/dev/null 2>&1; then
	pycompile -p python-pykmip 
fi

# End automatically added section


exit 0
