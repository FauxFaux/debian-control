#!/bin/sh

set -e

if [ "$1" = "configure" ] ; then
	update-alternatives --install /usr/bin/swift swift /usr/bin/python2-swift 300 \
		--slave /usr/share/man/man1/swift.1.gz swift.1.gz /usr/share/man/man1/python2-swift.1.gz
fi


# Automatically added by dh_python2:
if which pycompile >/dev/null 2>&1; then
	pycompile -p python-swiftclient 
fi

# End automatically added section


exit 0
