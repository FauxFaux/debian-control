Depends: texlive-latex-base, texlive-latex-extra, libc6 (>= 2.27), libcfitsio7 (>= 3.450~), libfftw3-double3 (>= 3.3.5), libgsl23 (>= 2.5), libgslcblas0 (>= 2.4), libkpathsea6, libpng16-16 (>= 1.6.2-1), libreadline7 (>= 6.0), libtinfo6 (>= 6), libxml2 (>= 2.7.4), zlib1g (>= 1:1.1.4)
