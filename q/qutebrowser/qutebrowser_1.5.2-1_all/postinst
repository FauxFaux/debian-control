#!/bin/sh
set -e
update-alternatives --install /usr/bin/x-www-browser x-www-browser /usr/bin/qutebrowser 40

# Automatically added by dh_python3:
if which py3compile >/dev/null 2>&1; then
	py3compile -p qutebrowser 
fi
if which pypy3compile >/dev/null 2>&1; then
	pypy3compile -p qutebrowser  || true
fi

if which py3compile >/dev/null 2>&1; then
	py3compile -p qutebrowser /usr/share/qutebrowser
fi
if which pypy3compile >/dev/null 2>&1; then
	pypy3compile -p qutebrowser /usr/share/qutebrowser || true
fi

# End automatically added section
# Automatically added by dh_installmenu/11.4.1
if [ "$1" = "configure" ] && [ -x "`which update-menus 2>/dev/null`" ]; then
	update-menus
fi
# End automatically added section

