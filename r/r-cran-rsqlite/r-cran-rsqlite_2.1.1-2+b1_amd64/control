Depends: r-base-core (>= 3.5.0-5), r-api-3.5, r-cran-bit64, r-cran-blob (>= 1.1.1), r-cran-dbi (>= 1.0.0), r-cran-memoise, r-cran-pkgconfig, r-cran-rcpp (>= 0.12.7), libc6 (>= 2.14), libgcc1 (>= 1:3.0), libstdc++6 (>= 5.2)
