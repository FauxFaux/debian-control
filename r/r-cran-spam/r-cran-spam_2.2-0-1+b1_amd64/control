Depends: r-base-core (>= 3.5.1-1), r-api-3.5, r-cran-dotcall64, libblas3 | libblas.so.3, libc6 (>= 2.4), libgfortran5 (>= 8), liblapack3 | liblapack.so.3, libquadmath0 (>= 4.6)
