Depends: r-base-core (>= 3.5.0-5), r-api-3.5, r-cran-glue, r-cran-purrr, r-cran-rlang (>= 0.2.0), r-cran-rcpp (>= 0.12.0), libc6 (>= 2.14), libgcc1 (>= 1:3.0), libstdc++6 (>= 5.2)
