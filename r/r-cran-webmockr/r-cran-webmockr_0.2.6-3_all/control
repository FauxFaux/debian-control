Depends: r-base-core (>= 3.5.0-5), r-api-3.5, r-cran-curl, r-cran-jsonlite, r-cran-magrittr (>= 1.5), r-cran-lazyeval (>= 0.2.0), r-cran-r6 (>= 2.1.3), r-cran-urltools (>= 1.6.0), r-cran-fauxpas, r-cran-crul (>= 0.5.2)
