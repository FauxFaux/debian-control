Depends: r-base-core (>= 3.5.1-1), r-api-3.5, r-cran-rcolorbrewer, r-cran-rcpp (>= 0.9.4), libc6 (>= 2.14), libgcc1 (>= 1:3.0), libstdc++6 (>= 5.2)
