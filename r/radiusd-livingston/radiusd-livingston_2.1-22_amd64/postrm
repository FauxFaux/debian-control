#!/bin/sh
set -e

if [ "$1" = purge ] ; then
  if dpkg-statoverride --list /etc/radiusd-livingston 2>&1 | \
	grep -q "root root 750"
  then
	dpkg-statoverride --remove /etc/radiusd-livingston
  fi
fi

# Automatically added by dh_installinit/11.5.3
if [ "$1" = "purge" ] ; then
	update-rc.d radiusd-livingston remove >/dev/null
fi
# End automatically added section
# Automatically added by dh_installdebconf/11.5.3
if [ "$1" = purge ] && [ -e /usr/share/debconf/confmodule ]; then
	. /usr/share/debconf/confmodule
	db_purge
fi
# End automatically added section


exit 0
