#!/bin/sh

set -e


case "$1" in
    upgrade)
        /usr/share/perl6/rakudo-helper.pl --compiler-id > /var/lib/perl6/previous-compiler-id
    ;;

    remove|deconfigure|failed-upgrade)
        # nothing to do
    ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac



exit 0
