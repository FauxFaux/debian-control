		deb-systemd-helper mask 'ibacm.service' 'ibacm.socket' >/dev/null || true
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge 'ibacm.service' 'ibacm.socket' >/dev/null || true
		deb-systemd-helper unmask 'ibacm.service' 'ibacm.socket' >/dev/null || true
	fi
