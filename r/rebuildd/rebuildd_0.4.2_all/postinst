#!/bin/sh
set -e

# Automatically added by dh_python2:
if which pycompile >/dev/null 2>&1; then
	pycompile -p rebuildd 
fi

# End automatically added section
# Automatically added by dh_installinit
if [ -x "/etc/init.d/rebuildd" ]; then
	update-rc.d rebuildd defaults >/dev/null
	invoke-rc.d rebuildd start || exit $?
fi
# End automatically added section
# Automatically added by dh_installinit
if [ -x "/etc/init.d/rebuildd-httpd" ]; then
	update-rc.d rebuildd-httpd defaults >/dev/null
	invoke-rc.d rebuildd-httpd start || exit $?
fi
# End automatically added section
