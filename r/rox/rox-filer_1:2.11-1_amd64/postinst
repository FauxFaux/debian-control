#!/bin/sh

set -e

if [ "$1" = "configure" ]; then
	if [ -x /usr/bin/update-mime-database ]; then
		update-mime-database /usr/share/mime/ ;
	fi
fi

# Automatically added by dh_installmenu
if [ "$1" = "configure" ] && [ -x "`which update-menus 2>/dev/null`" ]; then
	update-menus
fi
# End automatically added section


