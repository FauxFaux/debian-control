#!/bin/sh
set -e

# remove menu-methods generated menu scripts
if [ "$1" = "remove" ] &&
   [ -f /etc/menu-methods/rxvt ] &&
   [ -x "`which install-menu 2>/dev/null`" ]
then
  install-menu --remove /etc/menu-methods/rxvt
fi

# Automatically added by dh_installmenu
inst=/etc/menu-methods/rxvt
if [ "$1" = "remove" ] && [ -f "$inst" ]; then chmod a-x $inst ; fi
if [ -x "`which update-menus 2>/dev/null`" ]; then update-menus ; fi
# End automatically added section

