#!/bin/sh
set -e
# Automatically added by dh_installinit/11.4
if [ "$1" = "purge" ] ; then
	update-rc.d sbd remove >/dev/null
fi
# End automatically added section
# Automatically added by dh_installsystemd/11.4
if [ -d /run/systemd/system ]; then
	systemctl --system daemon-reload >/dev/null || true
fi
# End automatically added section
# Automatically added by dh_installsystemd/11.4
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask 'sbd.service' 'sbd_remote.service' >/dev/null || true
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge 'sbd.service' 'sbd_remote.service' >/dev/null || true
		deb-systemd-helper unmask 'sbd.service' 'sbd_remote.service' >/dev/null || true
	fi
fi
# End automatically added section
