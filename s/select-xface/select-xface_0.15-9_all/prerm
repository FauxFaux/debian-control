#!/bin/sh
set -e
if [ ! -e /var/lib/emacsen-common/state/package/installed/emacsen-common ]; then
    # Fallback to emacsen-common 1.x
    if [ -x /usr/lib/emacsen-common/emacs-package-remove ]; then
	/usr/lib/emacsen-common/emacs-package-remove select-xface
    fi
else
# Automatically added by dh_installemacsen
if [ -e /var/lib/emacsen-common/state/package/installed/emacsen-common -a -x /usr/lib/emacsen-common/emacs-package-remove ] ; then
	/usr/lib/emacsen-common/emacs-package-remove --prerm select-xface
fi
# End automatically added section

fi
