#!/bin/sh

set -e

# This package does not provide anything since 3.0.2+dfsg1-1
if [ "$1" = upgrade ] &&
   dpkg --compare-versions "$2" lt-nl 3.0.2+dfsg1-1~ &&
   [ -e /usr/share/apache2/apache2-maintscript-helper ]; then
	. /usr/share/apache2/apache2-maintscript-helper
	(
		apache2_msg info "libapache2-mod-shib2: faking a postrm purge for apache2_invoke"
		APACHE2_MAINTSCRIPT_NAME=postrm
		APACHE2_MAINTSCRIPT_METHOD=purge
		apache2_needs_action() false # do not restart or reload Apache
		apache2_invoke disconf shib2
		apache2_invoke dismod  shib2
	)
fi

# Automatically added by dh_installdeb/11.5.4
dpkg-maintscript-helper rm_conffile /etc/apache2/mods-available/shib2.load 3.0.2\+dfsg1-1\~ libapache2-mod-shib2 -- "$@"
dpkg-maintscript-helper mv_conffile /etc/apache2/conf-available/shib2.conf /etc/apache2/conf-available/shib.conf 3.0.2\+dfsg1-1\~ libapache2-mod-shib2 -- "$@"
dpkg-maintscript-helper mv_conffile /etc/logcheck/ignore.d.server/libapache2-mod-shib2 /etc/logcheck/ignore.d.server/libapache2-mod-shib 3.0.2\+dfsg1-1\~ libapache2-mod-shib2 -- "$@"
# End automatically added section


exit 0
