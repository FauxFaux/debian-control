	pypy3compile -p python3-silx  || true
fi

# End automatically added section

# Automatically added by dh_python3:
if which py3compile >/dev/null 2>&1; then
	py3compile -p python3-silx /usr/lib/x86_64-linux-gnu/qt5/plugins/designer/python
fi
if which pypy3compile >/dev/null 2>&1; then
	pypy3compile -p python3-silx /usr/lib/x86_64-linux-gnu/qt5/plugins/designer/python || true
fi

# End automatically added section
