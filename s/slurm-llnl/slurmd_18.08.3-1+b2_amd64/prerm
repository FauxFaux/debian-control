#!/bin/sh

set -e

case "$1" in
	remove|deconfigure)
		update-alternatives --remove slurmd /usr/sbin/slurmd-wlm
		;;
esac

# Automatically added by dh_installinit/11.5.3
if [ -x "/etc/init.d/slurmd" ] && [ "$1" = remove ]; then
	invoke-rc.d slurmd stop || exit 1
fi
# End automatically added section


exit 0
