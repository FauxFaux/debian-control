#! /bin/sh
set -e

if dpkg-maintscript-helper supports mv_conffile 2>/dev/null; then
    dpkg-maintscript-helper mv_conffile /etc/startup.sh /etc/smcroute/startup.sh 2.0.0-4 -- "$@"
fi

# Automatically added by dh_installinit/10.7.2
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/smcroute" ]; then
		update-rc.d smcroute defaults >/dev/null
		invoke-rc.d smcroute start || exit $?
	fi
fi
# End automatically added section

