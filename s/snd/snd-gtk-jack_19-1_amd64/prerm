#!/bin/sh -e

set -e 



FLAVOR=".gtk-jack"

case "$1" in
    (remove|deconfigure)
        update-alternatives --remove snd /usr/bin/snd${FLAVOR}
    ;;
    (upgrade)
    ;;

    (failed-upgrade)
    ;;

    (*)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 0
    ;;
esac

exit 0
