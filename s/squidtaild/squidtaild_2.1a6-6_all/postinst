#!/bin/sh
set -e
# Automatically added by dh_installinit
if [ -x "/etc/init.d/squidtaild" ]; then
	update-rc.d squidtaild defaults >/dev/null
	if [ -x "`which invoke-rc.d 2>/dev/null`" ]; then
		invoke-rc.d squidtaild start || exit $?
	else
		/etc/init.d/squidtaild start || exit $?
	fi
fi
# End automatically added section
