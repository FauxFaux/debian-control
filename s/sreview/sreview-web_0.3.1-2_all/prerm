#!/bin/sh
set -e
# Automatically added by dh_installinit/11.3.2
if [ -x "/etc/init.d/sreview-web" ] && [ "$1" = remove ]; then
	invoke-rc.d sreview-web stop || exit 1
fi
# End automatically added section
# Automatically added by dh_systemd_start/11.3.2
if [ -d /run/systemd/system ] && [ "$1" = remove ]; then
	deb-systemd-invoke stop 'sreview-web.service' >/dev/null || true
fi
# End automatically added section
# Automatically added by dh_apache2/UNDECLARED
if [ "$1" = "remove" ] || [ "$1" = "deconfigure" ] ; then
	if true; then
		if [ -e /usr/share/apache2/apache2-maintscript-helper ] ; then
			. /usr/share/apache2/apache2-maintscript-helper

			for conf in sreview-web.conf  ; do
				apache2_invoke dissite $conf  || exit 1
			done
		fi
	fi
fi
# End automatically added section
