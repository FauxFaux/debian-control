#! /bin/sh -e

invoke_failure() {
    # invoke-rc.d failed
    return
}

# Automatically added by dh_systemd_start/11
if [ -d /run/systemd/system ] && [ "$1" = remove ]; then
	deb-systemd-invoke stop 'sssd-secrets.service' 'sssd-secrets.socket' >/dev/null
fi
# End automatically added section
# Automatically added by dh_installinit/11
if [ -x "/etc/init.d/sssd" ] && [ "$1" = remove ]; then
	invoke-rc.d sssd stop || invoke_failure
fi
# End automatically added section


