#!/bin/sh
set -e


case "$1" in
    configure)
     for file in /usr/lib/stardata-common/*; do
      	if [ -x $file ] ; then
		name=`basename $file`
		register-stardata install all $name
	fi
     done

    ;;
    
    abort-upgrade|abort-remove|abort-deconfigure)

    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac


exit 0

