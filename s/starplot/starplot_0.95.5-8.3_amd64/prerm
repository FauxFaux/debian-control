#!/bin/sh
set -e




case "$1" in
    remove|upgrade|deconfigure)
    if [ -x /usr/sbin/register-stardata ] ; then
       register-stardata remove all starplot
    fi
    ;;

    failed-upgrade)
        ;;
    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

exit 0