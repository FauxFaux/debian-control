#!/bin/sh
set -e
# Automatically added by dh_installinit/11.5.1
if [ "$1" = "purge" ] ; then
	update-rc.d suricata remove >/dev/null
fi
# End automatically added section
# Automatically added by dh_installdeb/11.5.1
dpkg-maintscript-helper mv_conffile /etc/suricata/suricata-debian.yaml /etc/suricata/suricata.yaml 3.1-1 suricata -- "$@"
# End automatically added section
# Automatically added by dh_installsystemd/11.5.1
if [ "$1" = "remove" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask 'suricata.service' >/dev/null || true
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge 'suricata.service' >/dev/null || true
		deb-systemd-helper unmask 'suricata.service' >/dev/null || true
	fi
fi
# End automatically added section
