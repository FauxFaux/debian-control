Depends: libnss3-tools, avahi-utils, unzip, zip, systemtap (= 3.3-1), systemtap-client (= 3.3-1), libavahi-client3 (>= 0.6.16), libavahi-common3 (>= 0.6.16), libc6 (>= 2.27), libgcc1 (>= 1:3.0), libnspr4 (>= 2:4.9-2~), libnss3 (>= 2:3.13.4-2~), libstdc++6 (>= 5.2), net-tools
