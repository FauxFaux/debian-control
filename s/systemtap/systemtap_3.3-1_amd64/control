Depends: systemtap-runtime (= 3.3-1), libavahi-client3 (>= 0.6.16), libavahi-common3 (>= 0.6.16), libc6 (>= 2.27), libdw1 (>= 0.167), libelf1 (>= 0.142), libgcc1 (>= 1:3.0), libnspr4 (>= 2:4.9-2~), libnss3 (>= 2:3.13.4-2~), libsqlite3-0 (>= 3.5.9), libstdc++6 (>= 6), systemtap-common (= 3.3-1), make, lsb-release
