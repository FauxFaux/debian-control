#!/bin/sh
set -e
remove_libvariant_alternative () {
	local libvariant=$1
	update-alternatives --remove libtachyon.so.0 /usr/lib/x86_64-linux-gnu/libtachyon-${libvariant}.so.0
	return 0
	}
case $1 in
	remove|deconfigure)
		for LIBVARIANT in serial; do
			remove_libvariant_alternative $LIBVARIANT
		done
		;;
	upgrade|failed-upgrade) ;;
	*) echo "prerm called with unknown argument \`$1'" >&2 ; exit 0 ;;
esac

exit 0
