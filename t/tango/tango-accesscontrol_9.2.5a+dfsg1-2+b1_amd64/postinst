#!/bin/sh
set -e
# Automatically added by dh_installinit/11.3.2
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ] ; then
	if [ -x "/etc/init.d/tango-accesscontrol" ]; then
		update-rc.d tango-accesscontrol defaults 25 >/dev/null
		invoke-rc.d tango-accesscontrol start || exit 1
	fi
fi
