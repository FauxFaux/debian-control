#! /bin/sh
set -e

# Automatically added by dh_installmenu/11.2.1
if [ "$1" = "configure" ] && [ -x "`which update-menus 2>/dev/null`" ]; then
	update-menus
fi
# End automatically added section
# Automatically added by dh_installdeb/11.2.1
dpkg-maintscript-helper rm_conffile /etc/gnome/config/telegnome 0.2.0-1~ -- "$@"
# End automatically added section


if [ -d /etc/gnome/config ]; then
    rmdir -p --ignore-fail-on-non-empty /etc/gnome/config
fi

exit 0
