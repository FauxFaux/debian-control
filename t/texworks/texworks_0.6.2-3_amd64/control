Depends: libc6 (>= 2.14), libgcc1 (>= 1:3.0), libhunspell-1.6-0, libpoppler-qt5-1 (>= 0.24.5), libpoppler74 (>= 0.63.0), libqt5concurrent5 (>= 5.6.0~rc), libqt5core5a (>= 5.10.0), libqt5dbus5 (>= 5.0.2), libqt5gui5 (>= 5.8.0), libqt5script5 (>= 5.6.0~beta), libqt5scripttools5 (>= 5.6.0~beta), libqt5widgets5 (>= 5.3.0), libqt5xml5 (>= 5.0.2), libstdc++6 (>= 5), libsynctex2, zlib1g (>= 1:1.1.4)
