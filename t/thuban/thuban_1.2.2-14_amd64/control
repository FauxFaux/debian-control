Depends: gdal-abi-2-3-0, libc6 (>= 2.14), libgcc1 (>= 1:3.0), libgdal20 (>= 2.0.1), libproj13 (>= 4.8.0), libstdc++6 (>= 5.2), libwxbase3.0-0v5 (>= 3.0.4+dfsg), libwxgtk3.0-0v5 (>= 3.0.4+dfsg), python (<< 2.8), python (>= 2.7), python2.7, python:any (>= 2.6.6-7~), gdal-bin, python-wxgtk3.0, python-sqlite
