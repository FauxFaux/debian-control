#!/bin/sh
set -e

TINYMCE_INSTALL_DIR="/usr/share/tinymce/www"

case "$1" in
  configure)
    chown -R root:root $TINYMCE_INSTALL_DIR 
    chmod 755 $TINYMCE_INSTALL_DIR
  ;;
  abort-upgrade|abort-remove|abort-deconfigure)
  ;;
  *)
    echo "postinst called with unknown argument \`$1'" >&2
    exit 1
    ;;
esac


