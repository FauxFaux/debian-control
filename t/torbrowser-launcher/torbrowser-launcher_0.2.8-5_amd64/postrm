#!/bin/sh
set -e
# Automatically added by dh_installdeb/10.10.7
dpkg-maintscript-helper rm_conffile /etc/apparmor.d/usr.bin.torbrowser-launcher 0.2.8-4~ torbrowser-launcher -- "$@"
# End automatically added section
# Automatically added by dh_installdeb/10.10.7
dpkg-maintscript-helper rm_conffile /etc/apparmor.d/torbrowser.start-tor-browser 0.2.8-5~ torbrowser-launcher -- "$@"
# End automatically added section
# Automatically added by dh_apparmor/2.11.1-3
if [ "$1" = "purge" ] && ! [ -e "/etc/apparmor.d/torbrowser.Browser.firefox" ] ; then
    rm -f "/etc/apparmor.d/disable/torbrowser.Browser.firefox" || true
    rm -f "/etc/apparmor.d/force-complain/torbrowser.Browser.firefox" || true
    rm -f "/etc/apparmor.d/local/torbrowser.Browser.firefox" || true
    rmdir /etc/apparmor.d/disable 2>/dev/null || true
    rmdir /etc/apparmor.d/local   2>/dev/null || true
    rmdir /etc/apparmor.d         2>/dev/null || true
fi
# End automatically added section
# Automatically added by dh_apparmor/2.11.1-3
if [ "$1" = "purge" ] && ! [ -e "/etc/apparmor.d/torbrowser.Tor.tor" ] ; then
    rm -f "/etc/apparmor.d/disable/torbrowser.Tor.tor" || true
    rm -f "/etc/apparmor.d/force-complain/torbrowser.Tor.tor" || true
    rm -f "/etc/apparmor.d/local/torbrowser.Tor.tor" || true
    rmdir /etc/apparmor.d/disable 2>/dev/null || true
    rmdir /etc/apparmor.d/local   2>/dev/null || true
    rmdir /etc/apparmor.d         2>/dev/null || true
fi
# End automatically added section
