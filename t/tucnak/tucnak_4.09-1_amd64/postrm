#!/bin/sh

set -e

if [ "$1" = "purge" ] ; then
	if getent group davac4 >/dev/null; then
		groupdel davac4 || true
	fi
fi

[ -L /etc/udev/rules.d/025_davac4.rules ] && rm /etc/udev/rules.d/025_davac4.rules

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.



exit 0
