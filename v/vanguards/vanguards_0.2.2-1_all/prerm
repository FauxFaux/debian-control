#!/bin/sh
set -e
# Automatically added by dh_installsystemd/11.3.5
if [ -d /run/systemd/system ] && [ "$1" = remove ]; then
	deb-systemd-invoke stop 'vanguards.service' >/dev/null || true
fi
# End automatically added section

# Automatically added by dh_pypy:
if which pypyclean >/dev/null 2>&1; then
	pypyclean -p vanguards 
else
	dpkg -L vanguards | perl -ne 's,/([^/]*)\.py$,/__pycache__/\1.*, or next; unlink $_ or die $! foreach glob($_)'
	find /usr/lib/pypy/dist-packages/ -type d -name __pycache__ -empty -print0 | xargs --null --no-run-if-empty rmdir
fi

# End automatically added section
