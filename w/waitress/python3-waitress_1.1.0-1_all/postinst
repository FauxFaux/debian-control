#!/bin/sh

set -e

if [ "$1" = "configure" ]
then
    update-alternatives --install /usr/bin/waitress-serve waitress-serve /usr/bin/waitress-serve-python3 300
fi


# Automatically added by dh_python3:
if which py3compile >/dev/null 2>&1; then
	py3compile -p python3-waitress 
fi

# End automatically added section

