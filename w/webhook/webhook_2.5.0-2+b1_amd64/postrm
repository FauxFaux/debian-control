	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper mask 'webhook.service' >/dev/null || true
	fi
fi

if [ "$1" = "purge" ]; then
	if [ -x "/usr/bin/deb-systemd-helper" ]; then
		deb-systemd-helper purge 'webhook.service' >/dev/null || true
		deb-systemd-helper unmask 'webhook.service' >/dev/null || true
	fi
fi
