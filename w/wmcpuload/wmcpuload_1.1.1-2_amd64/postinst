#!/bin/sh
set -e
# Automatically added by dh_installmenu/11.5.3
if [ "$1" = "configure" ] && [ -x "`which update-menus 2>/dev/null`" ]; then
	update-menus
fi
# End automatically added section
# Automatically added by dh_icons/11.5.3
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ] ; then
	if which update-icon-caches >/dev/null 2>&1 ; then
		update-icon-caches /usr/share/icons/wmcpuload-16x16.xpm /usr/share/icons/wmcpuload-32x32.xpm /usr/share/icons/wmcpuload-48x48.xpm
	fi
fi
# End automatically added section
