Depends: libjs-jquery-ui (>= 1.12.1+dfsg), libjs-prototype (>= 1.7.1), ucf, php, php-bcmath, php-gd, php-mbstring, php-xml, php-pgsql | php-mysql | php-sqlite3, ttf-dejavu-core
