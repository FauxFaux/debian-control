#!/bin/sh
set -e
# Automatically added by dh_installzope
if which dzhandle >/dev/null; then
    case "$1" in remove)
	# remove:
	# - unregister sucessfully removed product

	dzhandle postrm-product -m "$*" zope-replacesupport /usr/share/zope/Products/ReplaceSupport ReplaceSupport
    esac
fi
# End automatically added section
